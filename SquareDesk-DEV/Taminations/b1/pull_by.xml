<tamination title="Pull By">

  <tam title="Right Pull By" formation="Facing Couples" group=" ">
    <path>
      <move select="Pull Left" scaleX="2" scaleY=".5" beats="2"/>
      <move select="Extend Right" scaleX="2" scaleY=".5" beats="2"/>
    </path>
    <path>
      <move select="Pull Left" scaleX="2" scaleY=".5" beats="2"/>
      <move select="Extend Right" scaleX="2" scaleY=".5" beats="2"/>
    </path>
  </tam>

  <tam title="Left Pull By" formation="Facing Couples" group=" ">
    <path>
      <move select="Pull Right" scaleX="2" scaleY=".5" beats="2"/>
      <move select="Extend Left" scaleX="2" scaleY=".5" beats="2"/>
    </path>
    <path>
      <move select="Pull Right" scaleX="2" scaleY=".5" beats="2"/>
      <move select="Extend Left" scaleX="2" scaleY=".5" beats="2"/>
    </path>
  </tam>

</tamination>
