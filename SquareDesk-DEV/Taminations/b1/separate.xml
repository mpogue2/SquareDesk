<tamination title="Separate">

  <tam title="Head Couples Separate And Star Thru"
       formation="Static Square" group="Head Couples Separate">
    <path>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2.5"/>
      <move select="Forward"/>
      <move select="Quarter Right" hands="gripright" offsetX="1" offsetY="-.5"/>
    </path>
    <path>
      <move select="Extend Right" beats="3" scaleX="2" scaleY="1.5"/>
      <move select="Forward"/>
      <move select="Quarter Left" hands="gripright" offsetX="1" offsetY="-.5"/>
    </path>
    <path>
      <move select="Forward 2" hands="right" beats="3"/>
    </path>
    <path>
      <move select="Forward 2" hands="left" beats="3"/>
    </path>
  </tam>

  <tam title="Head Couples Pass Thru, Separate Around 1 to a Line" from=""
       formation="Static Square" group="Head Couples Pass Thru, Separate">
    <path>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Run Left" beats="4" scaleY="1.5"/>
      <move select="Lead Left" beats="3" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Run Right" beats="4" scaleY="1.5"/>
      <move select="Lead Right" beats="3" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Stand" hands="right" beats="9"/>
      <move select="Extend Left" beats="4" scaleY="2"/>
    </path>
    <path>
      <move select="Stand" hands="left" beats="9"/>
      <move select="Extend Right" beats="4" scaleY="2"/>
    </path>
  </tam>

  <tam title="Head Couples Pass Thru, Separate Around 1 and Come Into the Middle" from=""
       formation="Static Square" group="Head Couples Pass Thru, Separate">
    <path>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Run Left" beats="4" scaleY="1.5"/>
      <move select="Lead Left" beats="3" scaleX="2" scaleY="2"/>
      <move select="Forward"/>
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Run Right" beats="4" scaleY="1.5"/>
      <move select="Lead Right" beats="3" scaleX="2" scaleY="2"/>
      <move select="Forward"/>
    </path>
    <path>
      <move select="Stand" hands="right" beats="8"/>
      <move select="Dodge Left" beats="2" scaleY=".75"/>
      <move select="Stand" beats="2"/>
      <move select="Dodge Right" beats="2" scaleY=".75"/>
    </path>
    <path>
      <move select="Stand" hands="left" beats="8"/>
      <move select="Dodge Right" beats="2" scaleY=".75"/>
      <move select="Stand" beats="2"/>
      <move select="Dodge Left" beats="2" scaleY=".75"/>
    </path>
  </tam>

  <tam title="Head Couples Pass Thru, Separate Around 2 to a Line" from=""
       formation="Static Square" group="Head Couples Pass Thru, Separate">
    <path>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Run Left" beats="4" scaleY="1.5" offsetX="-1"/>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Left" beats="3" scaleX="3" scaleY="1.5"/>
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Run Right" beats="4" scaleY="1.5" offsetX="-1"/>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Right" beats="3" scaleX="3" scaleY="2.5"/>
    </path>
    <path>
      <move select="Stand" beats="6"/>
      <move select="Forward" hands="right" beats="3"/>
    </path>
    <path>
      <move select="Stand" beats="6"/>
      <move select="Forward" hands="left" beats="3"/>
    </path>
  </tam>

  <tam title="Head Couples Pass Thru, Separate Around 2; Meet Your Partner and Dosado"
       formation="Static Square" group="Head Couples Pass Thru, Separate">
    <path>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Run Left" beats="4" scaleY="1.5" offsetX="-1"/>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Left" beats="3" scaleX="3" scaleY="1.5"/>
      <move select="Extend Left" scaleX="2" scaleY=".5" />
      <move select="Extend Right" scaleY=".5" />
      <move select="Retreat Right" scaleY=".5" />
      <move select="Retreat Left" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Run Right" beats="4" scaleY="1.5" offsetX="-1"/>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Right" beats="3" scaleX="3" scaleY="2.5"/>
      <move select="Extend Left" scaleX="2" scaleY=".5" />
      <move select="Extend Right" scaleY=".5" />
      <move select="Retreat Right" scaleY=".5" />
      <move select="Retreat Left" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Stand" beats="6"/>
      <move select="Forward" hands="right" beats="3"/>
    </path>
    <path>
      <move select="Stand" beats="6"/>
      <move select="Forward" hands="left" beats="3"/>
    </path>
  </tam>

  <tam title="Outside Couples Separate And Touch a Quarter"
       formation="Trade By" group="Outside Couples Separate">
  <path>
    <move select="Run Right" beats="5" offsetX="-3" offsetY="-.5"/>
    <move select="Hinge Right" scaleY=".5"/>
  </path>
  <path>
    <move select="Run Left" beats="5" offsetX="-3" offsetY="-.5"/>
    <move select="Hinge Right" scaleY=".5"/>
  </path>
  <path>
  </path>
  <path>
  </path>
  </tam>

  <tam title="Heads Pass the Ocean and Swing Thru, Others Separate and Everybody Right and Left Thru"
       formation="Static Square" group="">
    <path>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5" />
      <move select="Lead Right" beats="2" scaleX="1.5" scaleY="1.5" />
      <move select="Swing Right" beats="2" scaleX=".5" scaleY=".5" />
      <move select="Swing Left" beats="2" scaleX=".5" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
      <move select="Beau Wheel" scaleX=".67" />
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5" />
      <move select="Lead Left" hands="left" beats="2" scaleX=".5" scaleY=".5" />
      <move select="Swing Right" beats="2" scaleX=".5" scaleY=".5" />
      <move select="Stand" beats="2" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
      <move select="Belle Wheel" scaleX=".67" />
    </path>
    <path>
      <move select="Quarter Left" beats="3" offsetY="1" />
      <move select="Lead Right" beats="5" scaleX="1.5" scaleY="3" />
      <move select="Stand" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
      <move select="Beau Wheel" scaleX=".67" />
    </path>
    <path>
      <move select="Quarter Right" beats="3" offsetY="-1" />
      <move select="Lead Left" beats="5" scaleX=".5" scaleY="3" />
      <move select="Stand" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
      <move select="Belle Wheel" scaleX=".67" />
    </path>
  </tam>

</tamination>
