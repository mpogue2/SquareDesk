<tamination title="Anything Concept">

  <tam title="Split Counter Coordinate" from="" group=" " formation="Ocean Waves RH BGGB">
    <path couples="2 4">
      <move select="Quarter Right" hands="gripright" beats="4" offsetX="3" />
      <move select="Forward" />
      <move select="Hinge Right" scaleX=".5" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
    <path couples="2 4">
      <move select="Quarter Right" hands="gripright" beats="4" offsetX="-1" offsetY="-2" />
      <move select="Lead Right" scaleX=".5" />
      <move select="Lead Right" scaleX="3" scaleY="1.5" beats="3" />
    </path>
    <path couples="3 1">
      <move select="Quarter Right" hands="gripright" beats="4" offsetX="3" />
      <move select="Forward"/>
      <move select="Hinge Right" scaleX=".5" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
    <path couples="3 1">
      <move select="Quarter Right" hands="gripright" beats="4" offsetX="-1" offsetY="-2" />
      <move select="Forward"/>
      <move select="Hinge Right" scaleX=".5" />
      <move select="Lead Right" scaleX="3" scaleY="2.5" beats="3" />
    </path>
  </tam>

  <tam title="Split Coordinate"
       from="" group=" "
       formation="Column RH GBGB">
    <path>
      <move select="Flip Right" beats="2" />
      <move select="Forward" />
      <move select="Hinge Right" scaleX=".5" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
    <path>
      <move select="Forward" scaleX="2" beats="2" />
      <move select="Lead Right" scaleX=".5" />
      <move select="Lead Right" scaleX="3" scaleY="1.5" beats="3" />
    </path>
    <path>
      <move select="Run Right" beats="2" scaleX=".5"/>
      <move select="Forward"/>
      <move select="Hinge Right" scaleX=".5" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
    <path>
      <move select="Forward" scaleX="3" beats="3" />
      <move select="Hinge Right" scaleX=".5" />
      <move select="Lead Right" scaleX="3" scaleY="2.5" beats="3" />
    </path>
  </tam>

  <tam title="Counter Motivate" from="" formation="Ocean Waves RH BGGB" group=" ">
    <path>
      <move select="Lead Right" beats="5" scaleX="5" />
      <move select="Extend Right" beats="4.5" scaleX="2" scaleY="2" />
      <move select="Swing Right" />
      <move select="Extend Left" beats="4.5" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Quarter Left" hands="gripleft" beats="5" offsetX="-1" offsetY="3" />
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Cast Left" />
    </path>
    <path>
      <move select="Quarter Left" hands="gripleft" beats="5" offsetX="3" offsetY="-1" />
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Lead Right" beats="5" scaleY="5" />
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
      <move select="Swing Right" />
      <move select="Cast Left" />
    </path>
  </tam>

  <tam title="Trade Motivate" from="" formation="Ocean Waves LH BGGB" group=" ">
  <path>
    <!-- Trade Circulate -->
      <move select="Run Left" beats="4" scaleX="1.5" scaleY="2" />
    <!-- finish Motivate -->
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
  </path>
  <path>
    <!-- Trade Circulate -->
      <move select="Extend Left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    <!-- finish Motivate -->
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
      <move select="Swing Right" />
      <move select="Cast Left" />
  </path>
  <path>
    <!-- Trade Circulate -->
      <move select="Run Right" beats="4" scaleX=".5" scaleY="2" />
    <!-- finish Motivate -->
      <move select="Extend Right" beats="4.5" scaleX="2" scaleY="2" />
      <move select="Swing Right" />
      <move select="Extend Left" beats="4.5" scaleX="2" scaleY="2" />
  </path>
  <path>
    <!-- Trade Circulate -->
      <move select="Extend Left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    <!-- finish Motivate -->
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Cast Left" />
  </path>
  </tam>

  <tam title="Counter Percolate" from="" formation="Ocean Waves RH BGGB" group=" ">
    <path>
      <move select="Lead Right" beats="5" scaleX="5" />
      <move select="Forward 2" />
      <move select="Hinge Right" />
      <move select="Extend Right" hands="left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Quarter Left" hands="gripleft" beats="5" offsetX="-1" offsetY="3" />
      <move select="Lead Left" beats="2" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" />
    </path>
    <path>
      <move select="Quarter Left" hands="gripleft" beats="5" offsetX="3" offsetY="-1" />
      <move select="Forward 2" />
      <move select="Hinge Right" />
      <move select="Stand" hands="none" beats="4"/>
    </path>
    <path>
      <move select="Lead Right" beats="5" scaleY="5" />
      <move select="Lead Right" beats="2" scaleX="2" scaleY="3" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="Split Percolate" from="" formation="Ocean Waves RH BGGB" group=" ">
    <path>
      <move select="Forward 4"/>
      <move select="Lead Right" beats="2" scaleX="2" scaleY="3" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Run Right" beats="4"/>
      <move select="Forward 2" />
      <move select="Hinge Right" />
      <move select="Extend Right" hands="left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Forward 4"/>
      <move select="Lead Left" beats="2" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" />
    </path>
    <path>
      <move select="Run Right" beats="4"/>
      <move select="Forward 2" />
      <move select="Hinge Right" />
      <move select="Stand" hands="none" beats="4"/>
    </path>
  </tam>

  <tam title="Trade Percolate" from="" formation="Ocean Waves LH BGGB" group=" ">
    <path>
    <!-- Trade Circulate -->
      <move select="Run Left" beats="4" scaleX="1.5" scaleY="2" />
    <!-- finish Perecolate -->
      <move select="Forward 2" />
      <move select="Hinge Right" />
      <move select="Stand" hands="none" beats="4"/>
    </path>
    <path>
    <!-- Trade Circulate -->
      <move select="Extend Left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    <!-- finish Percolate -->
      <move select="Lead Right" beats="2" scaleX="2" scaleY="3" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Forward 2" />
    </path>
    <path>
    <!-- Trade Circulate -->
      <move select="Run Right" beats="4" scaleX=".5" scaleY="2" />
    <!-- finish Percolate -->
      <move select="Forward 2" />
      <move select="Hinge Right" />
      <move select="Extend Right" hands="left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
    <!-- Trade Circulate -->
      <move select="Extend Left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    <!-- finish Percolate -->
      <move select="Lead Left" beats="2" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" />
    </path>
  </tam>

</tamination>
