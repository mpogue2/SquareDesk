<tamination title="Reverse Cut the Galaxy">
  
  

  <tam title="Reverse Cut the Galaxy" from="Right-Hand Galaxy" formation="Galaxy RH GP">
    <path>
	  <move select="Hinge Right" scaleX=".5"/>
	  <move select="Lead Right" beats="3" scaleX="3" scaleY=".5"/>
    </path>
    <path>
	  <move select="Lead Right" beats="4.5" scaleY="2"/>
    </path>
    <path>
	  <move select="Hinge Right" scaleX=".5"/>
	  <move select="Lead Right" beats="3" scaleX="3" scaleY=".5"/>
    </path>
    <path>
	  <move select="Dodge Right" offsetX="-.5"/>
	  <move select="Forward" beats="1.5" scaleX="1.5"/>
    </path>
  </tam>

