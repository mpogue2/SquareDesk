<tamination title="As Couples Concept">

  <tam title="As Couples Cast Off Three Quarters" group="As Couples"
       formation="Tidal Line LH">
    <path>
      <move select="Quarter Right" beats="2"/>
      <move select="Quarter Right" beats="2"/>
      <move select="Quarter Right" beats="3" hands="both" offsetX="-1"/>
    </path>
    <path>
      <move select="BackHinge Left" beats="2"/>
      <move select="BackHinge Left" beats="2"/>
      <move select="BackHinge Left" beats="3" scaleX="3"/>
    </path>
    <path>
      <move select="Swing Right" beats="4" scaleX="2" scaleY="2"/>
      <move select="Hinge Right" beats="3" scaleX="3" scaleY="2"/>
    </path>
    <path>
      <move select="Swing Right" beats="4"/>
      <move select="Hinge Right" beats="3" hands="both"/>
    </path>
  </tam>

  <tam title="As Couples Chase Right" group="As Couples"
       formation="Lines Facing Out">
    <path>
      <move select="Beau Wheel" reflect="-1" offsetX="-2" />
      <move select="Forward 2" hands="left" />
      <move select="Run Right" hands="left" beats="3" />
    </path>
    <path>
      <move select="Belle Wheel" reflect="-1" offsetX="-2" />
      <move select="Forward 2" hands="right" />
      <move select="Run Right" hands="right" beats="3" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Run Right" hands="left" beats="3" />
      <move select="Forward 4" hands="left" />
    </path>
    <path>
      <move select="Run Right" hands="right" beats="3" scaleX="3" scaleY="3" />
      <move select="Forward 4" hands="right" />
    </path>
  </tam>

  <tam title="As Couples Centers Cross Run" group="As Couples"
       formation="Tidal Line RH">
    <path>
      <move select="Dodge Right" />
    </path>
    <path>
      <move select="Dodge Right" />
    </path>
    <path>
      <move select="Run Right" hands="right" scaleX="2" scaleY="2.5" />
    </path>
    <path>
      <move select="Run Right" hands="left" scaleY="1.5" />
    </path>
  </tam>

  <tam title="As Couples Crossfire" group="As Couples"
       formation="Tidal Line RH">
    <path>
      <move select="Run Right" hands="right" beats="4" scaleX="3.25" scaleY="3.25" offsetX="2" />
    </path>
    <path>
      <move select="Run Right" hands="left" beats="4" scaleX="1.75" scaleY="1.75" offsetX="2" />
    </path>
    <path>
      <move select="Swing Right" scaleX="1.5" scaleY="1.5" />
      <move select="Extend Left" hands="right" beats="2" scaleX="2" scaleY="1.5" />
    </path>
    <path>
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Extend Left" hands="left" beats="2" scaleX="2" scaleY=".5" />
    </path>
  </tam>

  <tam title="As Couples Dixie Style to a Wave" group="As Couples"
       formation="Normal Lines">
    <path>
      <move select="BackSashay Right" beats="6" hands="right" scaleY="2"/>
      <move select="Hinge Left" hands="both" beats="2" scaleX=".5" />
    </path>
    <path>
      <move select="BackSashay Right" beats="6" hands="left" scaleY="2"/>
      <move select="Hinge Left" beats="2" scaleX="1.5" scaleY="3" />
    </path>
    <path>
      <move select="Extend Left" hands="gripright" beats="3" scaleX="2" scaleY="2.5" />
      <move select="Extend Right" hands="gripright" beats="3" scaleX="2" scaleY="2.5" />
      <move select="Hinge Left" hands="both" beats="2" scaleX=".5" />
    </path>
    <path>
      <move select="Extend Left" hands="gripleft" beats="3" scaleX="2" scaleY="3.5" />
      <move select="Extend Right" hands="gripleft" beats="3" scaleX="2" scaleY="3.5" />
      <move select="Hinge Left" beats="2" scaleX="1.5" scaleY="3" />
    </path>
  </tam>

  <tam title="As Couples Explode the Wave" group="As Couples"
       formation="Two-Faced Tidal Line RH">
    <path>
      <move select="Lead Right" beats="3" scaleX="3.5" scaleY="3.5" />
      <move select="Extend Right" hands="right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX="2.5" scaleY="2.5" />
      <move select="Extend Right" hands="left" beats="2" scaleX="2" scaleY="1.5" />
    </path>
    <path>
      <move select="Hinge Left" beats="3" scaleX="1.5" scaleY="1.5" />
      <move select="Extend Right" hands="left" beats="2" scaleX="2" scaleY="1.5" />
    </path>
    <path>
      <move select="Hinge Left" beats="3" scaleX=".5" scaleY=".5" />
      <move select="Extend Right" hands="right" beats="2" scaleX="2" scaleY=".5" />
    </path>
  </tam>
  
  <tam title="As Couples Extend" group="As Couples"
       formation="Quarter Lines RH">
    <path>
      <move select="Extend Left" hands="right" beats="2" scaleX="1.5" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Left" hands="left" beats="2" scaleX="1.5" scaleY="2"/>
    </path>
    <path>
      <move select="Forward" hands="left" beats="2" scaleX="1.5"/>
    </path>
    <path>
      <move select="Forward" hands="right" beats="2" scaleX="1.5"/>
    </path>
  </tam>

  <tam title="As Couples Fan the Top" group="As Couples"
       formation="Two-Faced Tidal Line RH">
    <path>
      <move select="Lead Right" beats="4.5" scaleX="3.5" scaleY="3.5" />
    </path>
    <path>
      <move select="Lead Right" beats="4.5" scaleX="2.5" scaleY="2.5" />
    </path>
    <path>
      <move select="Swing Left" scaleX="1.5" scaleY="1.5" />
      <move select="Hinge Left" scaleX="1.5" scaleY="1.5" />
    </path>
    <path>
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="Hinge Left" scaleX=".5" scaleY=".5" />
    </path>
  </tam>

  <tam title="As Couples Ends Fold" group="As Couples"
       formation="Two-Faced Tidal Line RH">
    <path>
      <move select="Run Right" hands="right" offsetX="2" offsetY="-.5" />
    </path>
    <path>
      <move select="U-Turn Right" hands="left" offsetX="2" offsetY=".5" />
    </path>
    <path>
      <move select="Dodge Right" hands="left" scaleY=".75" />
    </path>
    <path>
      <move select="Dodge Right" hands="both" scaleY=".25" />
    </path>
  </tam>

  <tam title="As Couples Ends Cross Fold" group="As Couples"
       formation="Two-Faced Tidal Line RH">
    <path>
      <move select="Run Right" beats="5" hands="right" offsetX="2" offsetY="-4.5" />
    </path>
    <path>
      <move select="U-Turn Right" beats="5" hands="left" offsetX="2" offsetY="-3.5" />
    </path>
    <path>
      <move select="Dodge Right" beats="5" hands="left" scaleY=".75" />
    </path>
    <path>
      <move select="Dodge Right" beats="5" hands="both" scaleY=".25" />
    </path>
  </tam>

  <tam title="As Couples Flutterwheel" group="As Couples"
       formation="Normal Lines">
    <path>
      <move select="Stand" beats="2" hands="right" />
      <move select="Extend Right" hands="right" beats="3" />
      <move select="Counter Rotate Right 3 -1" hands="right" beats="3" offsetY="1"/>
      <move select="Counter Rotate Right 3 -1" hands="right" beats="3" offsetY="1" offsetX="2"/>
    </path>
    <path>
      <move select="Stand" beats="2" hands="left" />
      <move select="Forward" hands="left" beats="3" />
      <move select="Counter Rotate Right 2 0" hands="both" beats="3" offsetY="1"/>
      <move select="Counter Rotate Right 2 0" hands="both" beats="3" offsetY="1" offsetX="1"/>
    </path>
    <path>
      <move select="Extend Left" hands="gripright" beats="2" scaleX="2" scaleY="2.5" />
      <move select="Belle Wheel" reflect="-1" scaleX=".5" scaleY=".5" offsetX="1" offsetY="-.5" />
      <move select="Counter Rotate Right 1 2" beats="3" hands="both" />
      <move select="Counter Rotate Right 1 2" beats="3" hands="both" />
    </path>
    <path>
      <move select="Extend Left" hands="gripleft" beats="2" scaleX="2" scaleY="3.5" />
      <move select="Beau Wheel" reflect="-1" scaleX=".5" scaleY=".5" offsetX="1" offsetY="-.5" />
      <move select="Counter Rotate Right 0 3" beats="3" hands="left"/>
      <move select="Counter Rotate Right 0 3" beats="3" hands="left" offsetX="-1"/>
    </path>
  </tam>

  <tam title="As Couples Follow Your Neighbor" group="As Couples"
       formation="Two-Faced Lines RH">
    <path>
      <move select="Extend Right" hands="right" beats="2" scaleX="2" scaleY="1.5" />
      <move select="Swing Right" scaleX="1.5" scaleY="1.5" />
      <move select="Hinge Right" scaleX="1.5" scaleY="1.5" />
    </path>
    <path>
      <move select="Extend Right" hands="left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Hinge Right" scaleX=".5" scaleY=".5" />
    </path>
    <path>
      <move select="Lead Right" hands="left" beats="2" />
      <move select="U-Turn Right" offsetY=".5" />
    </path>
    <path>
      <move select="Lead Right" hands="right" beats="2" scaleX="2" scaleY="3" />
      <move select="Run Right" offsetY=".5" />
    </path>
  </tam>

  <tam title="As Couples Follow Your Neighbor" group="As Couples" display="none"
       formation="Two-Faced Lines LH">
    <path>
      <move select="Lead Left" hands="left" beats="2" scaleX="2" scaleY="3" />
      <move select="Run Left" offsetY="-.5" />
    </path>
    <path>
      <move select="Lead Left" hands="right" beats="2" />
      <move select="U-Turn Left" offsetY="-.5" />
    </path>
    <path>
      <move select="Extend Left" hands="right" beats="2" scaleX="2" scaleY=".5" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="Hinge Left" scaleX=".5" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" hands="left" beats="2" scaleX="2" scaleY="1.5" />
      <move select="Swing Left" scaleX="1.5" scaleY="1.5" />
      <move select="Hinge Left" scaleX="1.5" scaleY="1.5" />
    </path>
  </tam>

  <tam title="As Couples Half Sashay" group="As Couples"
       formation="Normal Lines" >
    <path>
      <move select="BackSashay Right" beats="6" hands="right" scaleX="1.5" scaleY="2"/>
    </path>
    <path>
      <move select="BackSashay Right" beats="6" hands="left" scaleX="1.5" scaleY="2"/>
    </path>
    <path>
      <move select="Sashay Left" beats="6" hands="right" scaleX="1.5" scaleY="2"/>
    </path>
    <path>
      <move select="Sashay Left" beats="6" hands="left" scaleX="1.5" scaleY="2"/>
    </path>
  </tam>

  <tam title="As Couples Ladies Center as the Men Sashay" group="As Couples">
     <formation>
      <dancer gender="boy" x="-2.414" y="1" angle="-22.5"/>
      <dancer gender="girl" x="1" y="2.414" angle="247.5"/>
      <dancer gender="boy" x="-2.414" y="-1" angle="22.5"/>
      <dancer gender="girl" x="-1" y="2.414" angle="292.5"/>
    </formation>
    <path>
      <move select="Sxtnth Left" />
      <move select="cl" />
      <move select="cl" />
      <move select="cl" />
      <move select="cl" />
      <move select="Sxtnth Right" />
    </path>
    <path>
      <move select="Forward" beats="2" hands="right" scaleX="2" offsetY=".2"/>
      <move select="Stand" beats=".5"/>
      <move select="Back" beats="2" hands="right" scaleX="2" offsetY="-.2"/>
    </path>
    <path>
      <move select="Sxtnth Left" />
      <move select="cl" />
      <move select="cl" />
      <move select="cl" />
      <move select="cl" />
      <move select="Sxtnth Right" />
    </path>
    <path>
      <move select="Forward" beats="2" hands="left" scaleX="2" offsetY="-.2"/>
      <move select="Stand" beats=".5"/>
      <move select="Back" beats="2" hands="left" scaleX="2" offsetY=".2"/>
    </path>
  </tam>

  <tam title="As Couples Left Swing Thru" group="As Couples"
       formation="Two-Faced Tidal Line LH">
    <path>
      <move select="Swing Left" scaleX="1.5" scaleY="1.5" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
    </path>
    <path>
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="Swing Right" scaleX="1.5" scaleY="1.5" />
    </path>
    <path>
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
    </path>
    <path>
      <move select="Swing Left" scaleX="1.5" scaleY="1.5" />
    </path>
  </tam>

  <tam title="As Couples Linear Cycle" group="As Couples"
       formation="Two-Faced Tidal Line RH">
    <path>
      <move select="Hinge Right" scaleX="1.5" scaleY="1.5" />
      <move select="Forward 4" />
      <move select="Run Right" beats="4" hands="right" scaleX="1.5" scaleY="2.25" />
    </path>
    <path>
      <move select="Hinge Right" scaleX=".5" scaleY=".5" />
      <move select="Forward 4" />
      <move select="Run Right" beats="4" hands="left" scaleX=".5" scaleY=".75" />
    </path>
    <path>
      <move select="Hinge Right" scaleX=".5" scaleY=".5" />
      <move select="Run Right" scaleX=".5" scaleY=".5" />
      <move select="Forward 4" />
      <move select="Beau Wheel" reflect="-1" offsetY=".5" />
    </path>
    <path>
      <move select="Hinge Right" scaleX="1.5" scaleY="1.5" />
      <move select="Run Right" scaleX="1.5" scaleY="1.5" />
      <move select="Forward 4" />
      <move select="Belle Wheel" reflect="-1" offsetY="1.5" />
    </path>
  </tam>

  <tam title="As Couples Pass the Ocean" group="As Couples"
       formation="Normal Lines">
    <path>
      <move select="Extend Left" hands="right" beats="2" scaleX="2" scaleY=".5" />
      <move select="Lead Right" hands="right" beats="4" scaleX="3.5" scaleY="3.5" />
    </path>
    <path>
      <move select="Extend Left" hands="left" beats="2" scaleX="2" scaleY="1.5" />
      <move select="Lead Right" hands="left" beats="4" scaleX="2.5" scaleY="2.5" />
    </path>
    <path>
      <move select="Extend Left" hands="right" beats="2" scaleX="2" scaleY=".5" />
      <move select="Lead Left" hands="right" beats="4" scaleX=".5" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" hands="left" beats="2" scaleX="2" scaleY="1.5" />
      <move select="Lead Left" hands="left" beats="4" scaleX="1.5" scaleY="1.5" />
    </path>
  </tam>

  <tam title="As Couples Peel Off" group="As Couples"
       formation="Two-Faced Lines RH">
    <path>
      <move select="Beau Wheel" hands="gripright" beats="5" offsetX="2" offsetY="-.5" />
    </path>
    <path>
      <move select="Belle Wheel" hands="gripleft" beats="5" offsetX="2" offsetY="-1.5" />
    </path>
    <path>
      <move select="Belle Wheel" hands="gripleft" beats="5" offsetX="-2" offsetY=".5" />
    </path>
    <path>
      <move select="Beau Wheel" hands="gripright" beats="5" offsetX="-2" offsetY="1.5" />
    </path>
  </tam>

  <tam title="As Couples Peel the Top" group="As Couples"
       formation="Two-Faced Lines RH">
    <path>
      <move select="Extend Right" hands="right" beats="2" scaleX="2" scaleY="1.5" />
      <move select="Swing Right" scaleX="1.5" scaleY="1.5" />
      <move select="Hinge Right" scaleX="1.5" scaleY="1.5" />
    </path>
    <path>
      <move select="Extend Right" hands="left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Hinge Right" scaleX=".5" scaleY=".5" />
    </path>
    <path>
      <move select="Belle Wheel" hands="gripleft" offsetX="-2" offsetY=".5" />
      <move select="Lead Left" beats="3.5" scaleX="3.5" scaleY="3.5" />
    </path>
    <path>
      <move select="Beau Wheel" hands="gripright" offsetX="-2" offsetY="1.5" />
      <move select="Lead Left" beats="3.5" scaleX="2.5" scaleY="2.5" />
    </path>
  </tam>

  <tam title="As Couples Recycle" group="As Couples"
       formation="Two-Faced Tidal Line RH">
    <path>
      <move select="Lead Right" beats="4" scaleX="3" scaleY="3.5" />
      <move select="Quarter Right" hands="right" beats="4" offsetX="3" offsetY="-1" />
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX="2" scaleY="2.5" />
      <move select="Quarter Right" hands="left" beats="4" offsetX="1" />
    </path>
    <path>
      <move select="Beau Wheel" reflect="-1" beats="4" scaleX=".5" scaleY=".5" offsetX="-1" offsetY="-.5" />
      <move select="Beau Wheel" reflect="-1" hands="gripleft" beats="4" offsetX="1" />
    </path>
    <path>
      <move select="Belle Wheel" reflect="-1" beats="4" scaleX=".5" scaleY=".5" offsetX="-1" offsetY="-.5" />
      <move select="Belle Wheel" reflect="-1" hands="gripright" beats="4" offsetX="1" offsetY="1" />
    </path>
  </tam>

  <tam title="As Couples Right and Left Thru" group="As Couples"
       formation="Normal Lines">
    <path>
      <move select="Extend Left" hands="right" beats="2" scaleX="2" scaleY=".5" />
      <move select="Extend Right" hands="right" beats="2" scaleX="2" scaleY="2" />
      <move select="Beau Wheel" scaleX="1.75" scaleY="2.25" />
    </path>
    <path>
      <move select="Extend Left" hands="left" beats="2" scaleX="2" scaleY="1.5" />
      <move select="Extend Right" hands="right" beats="2" scaleX="2" scaleY="2" />
      <move select="Beau Wheel" hands="both" scaleX=".58" scaleY=".75" />
    </path>
    <path>
      <move select="Extend Left" hands="right" beats="2" scaleX="2" scaleY=".5" />
      <move select="Forward 2" />
      <move select="Belle Wheel" hands="both" scaleX=".58" scaleY=".75" />
    </path>
    <path>
      <move select="Extend Left" hands="left" beats="2" scaleX="2" scaleY="1.5" />
      <move select="Forward 2" />
      <move select="Belle Wheel" scaleX="1.75" scaleY="2.25" />
    </path>
  </tam>

  <tam title="As Couples Centers Run" group="As Couples"
       formation="Tidal Line RH">
    <path>
      <move select="Dodge Right" />
    </path>
    <path>
      <move select="Dodge Right" />
    </path>
    <path>
      <move select="Run Left" hands="right" scaleY=".5" />
    </path>
    <path>
      <move select="Run Left" hands="left" scaleX="2" scaleY="1.5" />
    </path>
  </tam>

  <tam title="As Couples Scoot Back" group="As Couples"
       formation="Two-Faced Lines RH">
    <path>
      <move select="Extend Right" hands="right" beats="2" scaleX="2" scaleY="1.5" />
      <move select="Swing Right" scaleX="1.5" scaleY="1.5" />
      <move select="Extend Left" hands="right" beats="2" scaleX="2" scaleY="1.5" />
    </path>
    <path>
      <move select="Extend Right" hands="left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Extend Left" hands="left" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Run Right" hands="left" beats="5" />
    </path>
    <path>
      <move select="Run Right" hands="right" beats="5" scaleX="2.5" scaleY="3" />
    </path>
  </tam>

  <tam title="As Couples Scoot Back" group="As Couples" display="none"
       formation="Two-Faced Lines LH">
    <path>
      <move select="Run Left" hands="left" beats="5" scaleX="2.5" scaleY="3" />
    </path>
    <path>
      <move select="Run Left" hands="right" beats="5" />
    </path>
    <path>
      <move select="Extend Left" hands="right" beats="2" scaleX="2" scaleY=".5" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="Extend Right" hands="right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" hands="left" beats="2" scaleX="2" scaleY="1.5" />
      <move select="Swing Left" scaleX="1.5" scaleY="1.5" />
      <move select="Extend Right" hands="left" beats="2" scaleX="2" scaleY="1.5" />
    </path>
  </tam>

  <tam title="As Couples Slide Thru" group="As Couples">
     <formation>
      <dancer gender="boy" x="-2" y="3" angle="0"/>
      <dancer gender="girl" x="-2" y="-1" angle="0"/>
      <dancer gender="boy" x="-2" y="1" angle="0"/>
      <dancer gender="girl" x="-2" y="-3" angle="0"/>
    </formation>
    <path>
      <move select="Extend Left" beats="2" hands="right" scaleX="2" scaleY=".5" />
      <move select="Lead Right" beats="2" hands="right" scaleX="3" scaleY="1.5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" hands="right" scaleX="2" scaleY=".5" />
      <move select="Quarter Left" beats="2" hands="right" offsetX="1" offsetY="-1.5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" hands="left" scaleX="2" scaleY="1.5" />
      <move select="Lead Right" beats="2" hands="left" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" hands="left" scaleX="2" scaleY="1.5" />
      <move select="Quarter Left" beats="2" hands="left" offsetX="3" offsetY="-.5" />
    </path>
  </tam>

  <tam title="As Couples Spin the Top" group="As Couples"
       formation="Two-Faced Tidal Line RH">
    <path>
      <move select="Swing Right" scaleX="1.5" scaleY="1.5" />
