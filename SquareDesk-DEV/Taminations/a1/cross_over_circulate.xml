<tamination title="Cross Over Circulate">

  <tam title="Cross Over Circulate"
       from="Right-Hand Two-Faced Lines"
       formation="Two-Faced Lines RH"
       fractions="3" >
    <path numbers="1 8" couples="2 4">
      <move select="Extend Right" beats="3" scaleX="1.5" />
      <move select="Extend Right" beats="3" scaleX="2.5" />
    </path>
    <path numbers="2 7" couples="2 4">
      <move select="Extend Left" beats="3" scaleX="2.5" />
      <move select="Extend Left" beats="3" scaleX="1.5" />
    </path>
    <path numbers="3 6" couples="3 1">
      <move select="Quarter Right" offsetX=".1" offsetY="-1" />
      <move select="Forward 2" beats="3" />
      <move select="Quarter Right" offsetX="1" offsetY="-.1" />
    </path>
    <path numbers="4 5" couples="3 1">
      <move select="Quarter Right" offsetX=".1" offsetY="-1" />
      <move select="Forward 2" beats="3" />
      <move select="Quarter Right" offsetX="1" offsetY="-.1" />
    </path>
  </tam>

  <tam title="Cross Over Circulate"
       from="Left-Hand Two-Faced Lines"
       formation="Two-Faced Lines LH"
       fractions="3" >
    <path numbers="1 8">
      <move select="Quarter Left" offsetX=".1" offsetY="1" />
      <move select="Forward 2" beats="3" />
      <move select="Quarter Left" offsetX="1" offsetY=".1" />
    </path>
    <path numbers="2 7">
      <move select="Quarter Left" offsetX=".1" offsetY="1" />
      <move select="Forward 2" beats="3" />
      <move select="Quarter Left" offsetX="1" offsetY=".1" />
    </path>
    <path numbers="3 6">
      <move select="Extend Right" beats="3" scaleX="1.5" />
      <move select="Extend Right" beats="3" scaleX="2.5" />
    </path>
    <path numbers="4 5">
      <move select="Extend Left" beats="3" scaleX="2.5" />
      <move select="Extend Left" beats="3" scaleX="1.5" />
    </path>
  </tam>

  <tam title="Cross Over Circulate"
       from="Lines Facing Out"
       formation="Lines Facing Out"
       fractions="3" >
    <path>
      <move select="Quarter Left" offsetX=".1" offsetY="1" />
      <move select="Forward 2" beats="3" />
      <move select="Quarter Left" offsetX="1" offsetY=".1" />
    </path>
    <path>
      <move select="Quarter Left" offsetX=".1" offsetY="1" />
      <move select="Forward 2" beats="3" />
      <move select="Quarter Left" offsetX="1" offsetY=".1" />
    </path>
    <path>
      <move select="Lead Right" />
      <move select="Forward 2" beats="3" />
      <move select="Lead Right" />
    </path>
    <path>
      <move select="Lead Right" />
      <move select="Forward 2" beats="3" />
      <move select="Lead Right" />
    </path>
  </tam>

  <tam title="Cross Over Circulate"
       from="Facing Lines"
       formation="Normal Lines"
       fractions="3" >
    <path>
      <move select="Extend Left" scaleX="2" scaleY=".5" beats="2"/>
      <movement beats="4" hands="none" cx1="1" cy1="0" cx2="0" cy2="-2.5" x2="2" y2="-2.5"
                cx3="1" cx4="1" cy4="-.5" x4="2" y4="-.5"/>
    </path>
    <path>
      <move select="Extend Left" scaleX="2" scaleY=".5" beats="2"/>
      <movement beats="4" hands="none" cx1="1" cy1="0" cx2="1.5" cy2="1.5" x2="2" y2="1.5"
                cx3="1" cx4="1" cy4=".5" x4="2" y4=".5"/>
    </path>
    <path>
      <move select="Extend Left" scaleX="2" scaleY=".5" beats="2"/>
      <movement beats="4" hands="none" cx1="1" cy1="0" cx2="0" cy2="-2.5" x2="2" y2="-2.5"
                cx3="1" cx4="1" cy4="-.5" x4="2" y4="-.5"/>
    </path>
    <path>
      <move select="Extend Left" scaleX="2" scaleY=".5" beats="2"/>
      <movement beats="4" hands="none" cx1="1" cy1="0" cx2="1.5" cy2="1.5" x2="2" y2="1.5"
                cx3="1" cx4="1" cy4=".5" x4="2" y4=".5"/>
    </path>
  </tam>
  <tam title="Cross Over Circulate"
       from="Inverted Lines Ends Facing Out"
       formation="Inverted Lines Ends Facing Out">
    <path>
      <move select="Flip Left" beats="6" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="4.5" scaleX="3" scaleY="2" />
      <move select="Forward" beats="1.5" />
    </path>
    <path>
      <move select="Forward" beats="1.5" />
      <move select="Extend Right" beats="4.5" scaleX="3" scaleY="2" />
    </path>
    <path>
      <move select="Run Right" beats="6" scaleY="2" />
    </path>
  </tam>

  <tam title="Cross Over Circulate"
       from="Inverted Lines Ends Facing In"
       formation="Inverted Lines Ends Facing In">
    <path>
      <move select="Forward" beats="1.5" />
      <move select="Extend Right" beats="4.5" scaleX="3" scaleY="2" />
    </path>
    <path>
      <move select="Flip Left" beats="6" scaleY="2" />
    </path>
    <path>
      <move select="Run Right" beats="6" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="4.5" scaleX="3" scaleY="2" />
      <move select="Forward" beats="1.5" />
    </path>
  </tam>

  <tam title="Cross Over Circulate"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB"
       fractions="3" >
    <path numbers="1 8" couples="2 4">
      <move select="Forward" beats="1.5" />
      <move select="Extend Right" beats="4.5" scaleX="3" scaleY="2" />
    </path>
    <path numbers="2 7" couples="2 4">
      <move select="Run Left" beats="6" scaleX=".5" scaleY="2" />
    </path>
    <path numbers="3 6" couples="3 1">
      <move select="Forward" beats="1.5" />
      <move select="Extend Right" beats="4.5" scaleX="3" scaleY="2" />
    </path>
    <path numbers="4 5" couples="3 1">
      <move select="Run Right" beats="6" scaleX="1.5" scaleY="2" />
    </path>
  </tam>

  <tam title="Cross Over Circulate"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGBG"
       fractions="3" >
    <path>
      <move select="Run Left" beats="6" scaleX=".5" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="4" scaleX="2.5" scaleY="2" />
      <move select="Forward" scaleX="1.5" beats="2" />
    </path>
    <path>
      <move select="Run Right" beats="6" scaleX="1.5" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="4" scaleX="2.5" scaleY="2" />
      <move select="Forward" scaleX="1.5" beats="2" />
    </path>
  </tam>

  <tam title="Cross Over Circulate" from="3 and 1 lines #1"
       formation="3 and 1 lines #1">
    <path>
      <move select="Run Left" beats="6" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="3.5" scaleX="2" scaleY="2"/>
      <move select="Forward 2" beats="2.5"/>
    </path>
    <path>
      <move select="Stand" beats="2"/>
      <move select="Extend Right" beats="4" scaleX="4" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="3.5" scaleX="2" scaleY="2"/>
      <move select="Forward 2" beats="2.5"/>
    </path>
  </tam>

  <tam title="Cross Over Circulate" from="3 and 1 lines #2"
       formation="3 and 1 lines #2">
    <path>
      <move select="Forward 2" beats="2.5"/>
      <move select="Extend Right" beats="3.5" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Run Left" beats="6" scaleY="2"/>
    </path>
    <path>
      <move select="Stand" beats="2"/>
      <move select="Extend Right" beats="4" scaleX="4" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="3.5" scaleX="2" scaleY="2"/>
      <move select="Forward 2" beats="2.5"/>
    </path>
  </tam>

  <tam title="Cross Over Circulate" from="3 and 1 lines #3"
       formation="3 and 1 lines #3">
    <path>
      <move select="Stand" beats="2"/>
      <move select="Extend Right" beats="4" scaleX="4" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="3.5" scaleX="2" scaleY="2"/>
      <move select="Forward 2" beats="2.5"/>
    </path>
    <path>
      <move select="Run Right" beats="6" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="3.5" scaleX="2" scaleY="2"/>
      <move select="Forward 2" beats="2.5"/>
    </path>
  </tam>

  <tam title="Cross Over Circulate" from="3 and 1 lines #4"
       formation="3 and 1 lines #4">
    <path>
      <move select="Stand" beats="2"/>
      <move select="Extend Right" beats="4" scaleX="4" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="3.5" scaleX="2" scaleY="2"/>
      <move select="Forward 2" beats="2.5"/>
    </path>
    <path>
      <move select="Forward 2" beats="2.5"/>
      <move select="Extend Right" beats="3.5" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Run Right" beats="6" scaleY="2"/>
    </path>
  </tam>

  <tam title="Cross Over Circulate" from="3 and 1 lines #5"
       formation="3 and 1 lines #5">
    <path>
      <move select="Extend Right" beats="6" scaleX="4" scaleY="2"/>
    </path>
    <path>
      <move select="Run Left" beats="6" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Flip Right" beats="6" scaleY="2"/>
    </path>
    <path>
      <move select="Flip Right" beats="6" scaleY="2"/>
    </path>
  </tam>

  <tam title="Cross Over Circulate" from="3 and 1 lines #6"
       formation="3 and 1 lines #6">
    <path>
      <move select="Run Left" beats="6" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="6" scaleX="4" scaleY="2"/>
    </path>
    <path>
      <move select="Flip Right" beats="6" scaleY="2"/>
    </path>
    <path>
      <move select="Flip Right" beats="6" scaleY="2"/>
    </path>
  </tam>

  <tam title="Cross Over Circulate" from="3 and 1 lines #7"
       formation="3 and 1 lines #7">
    <path>
      <move select="Flip Left" beats="6" scaleY="2"/>
    </path>
    <path>
      <move select="Flip Left" beats="6" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Right" beats="6" scaleX="4" scaleY="2"/>
    </path>
    <path>
      <move select="Run Right" beats="6" scaleX="2" scaleY="2"/>
    </path>
  </tam>

  <tam title="Cross Over Circulate" from="3 and 1 lines #8"
       formation="3 and 1 lines #8">
    <path>
      <move select="Flip Left" beats="6" scaleY="2"/>
    </path>
    <path>
      <move select="Flip Left" beats="6" scaleY="2"/>
    </path>
    <path>
      <move select="Run Right" beats="6" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="6" scaleX="4" scaleY="2"/>
    </path>
  </tam>

</tamination>
