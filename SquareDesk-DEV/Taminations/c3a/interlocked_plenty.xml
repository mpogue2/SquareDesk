<tamination title="Interlocked Plenty">

  <tam title="Interlocked Plenty" from="Right-Hand Quarter Line"
       formation="Quarter Lines RH" fractions="6;6">
    <path>
      <move select="Extend Right" beats="6" scaleY="4" />
      <move select="Forward" beats="3" scaleX="3.5"/>
      <move select="Run Left"/>
      <move select="Forward" beats="2" scaleX="1.5"/>
    </path>
    <path>
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2" />
      <move select="Forward 3" />
      <move select="Run Left" offsetX="-.5" />
      <move select="Forward 3" />
      <move select="Run Left" offsetX="-1.5" />
    </path>
    <path>
      <move select="Forward 2" beats="6"/>
      <move select="Run Left" offsetX="-.5"/>
      <move select="Forward 3" />
      <move select="Run Left" beats="5" offsetX="1.5" scaleY="2"/>
    </path>
    <path>
      <move select="Run Right" beats="6" scaleY="2" offsetX="2"/>
      <move select="Forward" beats="3" scaleX="3.5"/>
      <move select="Run Left"/>
      <move select="Forward 3"/>
      <move select="Run Left" offsetX="1.5"/>
    </path>
  </tam>

  <tam title="Interlocked Plenty" from="Left-Hand Quarter Line"
       formation="Quarter Lines LH" fractions="6;7">
    <path>
      <move select="Extend Right" beats="6" scaleY="4" />
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5" />
      <move select="Forward" />
      <move select="Flip Left" scaleY=".75" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Extend Right" beats="2" scaleY="2" />
      <move select="Forward 4" />
      <move select="Flip Left" scaleY=".75" />
      <move select="Forward" />
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5" />
      <move select="Run Left" offsetX="-2"/>
    </path>
    <path>
      <move select="Stand" beats="4"/>
      <move select="Forward 2"/>
      <move select="Run Right" scaleY="1.25" />
      <move select="Forward" />
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5" />
      <move select="Run Right" beats="5" offsetX="1" scaleY="2"/>
    </path>
    <path>
      <move select="Run Left" beats="6" scaleY="2" offsetX="2"/>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5" />
      <move select="Forward" />
      <move select="Run Right" scaleY="1.25" />
      <move select="Extend Left" beats="2" scaleX="3"/>
      <move select="Run Right" offsetX="2" scaleY="1.5"/>
    </path>
  </tam>

  <tam title="Interlocked Plenty" from="Right-Hand 3/4 Line" fractions="6;7">
    <formation>
      <dancer gender="girl" x="-3" y="1" angle="180"/>
      <dancer gender="boy" x="-3" y="-1" angle="180"/>
      <dancer gender="girl" x="0" y="1" angle="0"/>
      <dancer gender="boy" x="0" y="3" angle="0"/>
    </formation>
    <path couples="3 1">
      <move select="Flip Right" offsetX="-2" />
      <move select="Forward 3"/>
      <move select="Run Right" scaleY="1.25" />
      <move select="Forward" />
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5" />
      <move select="Run Right" offsetX="-2"/>
    </path>
    <path couples="3 1">
      <move select="Flip Right" beats="6" scaleY="2" offsetX="-1"/>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5" />
      <move select="Forward" />
      <move select="Run Right" scaleY="1.25" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Stand" beats="4"/>
      <move select="Forward 2"/>
      <move select="Flip Left" scaleY=".75" />
      <move select="Forward" />
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5" />
      <move select="Run Left" beats="5" offsetX="1" scaleY="2"/>
    </path>
    <path>
      <move select="Run Right" beats="6" scaleY="2" offsetX="2"/>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5" />
      <move select="Forward" />
      <move select="Flip Left" scaleY=".75" />
      <move select="Extend Right" beats="2" scaleX="3"/>
      <move select="Run Left" offsetX="2" scaleY="1.5"/>
    </path>
  </tam>

  <tam title="Interlocked Plenty" from="Left-Hand 3/4 Line"
       formation="3/4 Lines LH" fractions="6;6">
    <path>
      <move select="Run Right" offsetX="-2" />
      <move select="Forward 3"/>
      <move select="Run Right"/>
      <move select="Forward 4" beats="3"/>
      <move select="Run Right" offsetX="-2"/>
    </path>
    <path>
      <move select="Run Right" beats="6" scaleY="2" offsetX="-1"/>
      <move select="Forward 4" beats="3"/>
      <move select="Run Right"/>
      <move select="Forward 2"/>
    </path>
    <path>
      <move select="Stand" beats="4"/>
      <move select="Forward 2"/>
      <move select="Run Right"/>
      <move select="Forward 4" beats="3"/>
      <move select="Run Right" beats="5" offsetX="1" scaleY="2"/>
    </path>
    <path>
      <move select="Run Left" beats="6" scaleY="2" offsetX="2"/>
      <move select="Forward 4" beats="3"/>
      <move select="Run Right"/>
      <move select="Forward 3" beats="2"/>
      <move select="Run Right" offsetX="2"/>
    </path>
  </tam>

  <tam title="Interlocked Scoot and Plenty" from="Right-Hand Quarter Line"
       formation="Quarter Lines RH" fractions="8;6;7">
    <path>
      <move select="Forward" beats="2" scaleX="1.5" />
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="1.5"/>
      <move select="Flip Right" offsetX="-2" />
      <move select="Forward 3"/>
      <move select="Run Right" scaleY="1.25" />
      <move select="Forward" />
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5" />
      <move select="Run Right" offsetX="-2"/>
    </path>
    <path>
      <move select="Forward" beats="2" scaleX="1.5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="1.5"/>
      <move select="Flip Right" beats="6" scaleY="2" offsetX="-1"/>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5" />
      <move select="Forward" />
      <move select="Run Right" scaleY="1.25" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Extend Right" beats="2" scaleX="1.5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Extend Left" beats="3" scaleX="1.5" scaleY="2"/>
      <move select="Run Right" beats="6" scaleY="2" offsetX="2"/>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5" />
      <move select="Forward" />
      <move select="Flip Left" scaleY=".75" />
      <move select="Extend Right" beats="2" scaleX="3"/>
      <move select="Run Left" offsetX="2" scaleY="1.5"/>
    </path>
    <path>
      <move select="Extend Right" beats="2" scaleX="1.5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Extend Left" beats="3" scaleX="1.5" scaleY="2"/>
      <move select="Stand" beats="4"/>
      <move select="Forward 2"/>
      <move select="Flip Left" scaleY=".75" />
      <move select="Forward" />
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5" />
      <move select="Run Left" beats="5" offsetX="1" scaleY="2"/>
    </path>
  </tam>

  <tam title="Interlocked Scoot and Plenty" from="Left-Hand Quarter Line"
       formation="Quarter Lines LH" fractions="8;6;6">
    <path>
      <move select="Forward" beats="2" scaleX="1.5" />
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Extend Left" beats="3" scaleX="1.5"/>
      <move select="Flip Left" beats="6" scaleY="2" offsetX="-1"/>

      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5" />
      <move select="Forward" />
      <move select="Flip Left" scaleY=".75" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Forward" beats="2" scaleX="1.5"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Extend Left" beats="3" scaleX="1.5"/>
      <move select="Flip Left" offsetX="-2" />
      <move select="Forward 3"/>

      <move select="Flip Left" scaleY=".75" />
      <move select="Forward" />
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5" />
      <move select="Run Left" offsetX="-2"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="1.5"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="1.5" scaleY="2"/>
      <move select="Run Left" beats="6" scaleY="2" offsetX="2"/>

      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5" />
      <move select="Forward" />
      <move select="Run Right" scaleY="1.25" />
      <move select="Extend Left" beats="2" scaleX="3"/>
      <move select="Run Right" offsetX="2" scaleY="1.5"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="1.5"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="1.5" scaleY="2"/>
      <move select="Stand" beats="4"/>
      <move select="Forward 2"/>

      <move select="Run Right" scaleY="1.25" />
      <move select="Forward" />
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5" />
      <move select="Run Right" beats="5" offsetX="1" scaleY="2"/>
    </path>
  </tam>


</tamination>
