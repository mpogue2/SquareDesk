<tamination title="Two-Faced Concept">

  <tam title="Two-Faced Alter and Circulate" group="Two-Faced"
       formation="Two-Faced Lines RH" fractions="3;7.5;4">
    <path>
      <move select="Run Right" />
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Swing Left" beats="4" />
      <move select="Forward 2" beats="1.5"/>
      <move select="Lead Left" />
    </path>
    <path>
      <move select="Flip Left" />
      <move select="U-Turn Left" />
      <move select="Forward 4" beats="4.5" />
      <move select="Run Right" beats="4" scaleX="1.5" scaleY="3" />
      <move select="Flip Right" />
    </path>
    <path>
      <move select="Flip Left" />
      <move select="U-Turn Left" />
      <move select="Run Right" beats="4.5" scaleX="3" scaleY="3" />
      <move select="Run Right" beats="4" scaleX="1.5" scaleY="3" />
      <move select="Flip Right" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Cast Left" />
      <move select="Stand" beats="3" />
      <move select="Swing Left" beats="4" />
      <move select="Forward 2" beats="1.5"/>
      <move select="Lead Left" />
    </path>
  </tam>

  <tam title="Two-Faced Alter the Wave" group="Two-Faced"
       formation="Two-Faced Line RH" fractions="3;4.5;4">
    <path>
      <move select="Run Right" />
      <move select="Cast Left" />
      <move select="Swing Left" beats="4" />
      <move select="Forward 2" beats="1.5"/>
      <move select="Lead Left" />
    </path>
    <path>
      <move select="Flip Left" />
      <move select="U-Turn Left" beats="4.5" />
      <move select="Lead Right" beats="2" scaleX="2" scaleY="3" />
      <move select="Lead Right" beats="2" scaleX="3" scaleY="2" />
      <move select="Flip Right" />
    </path>
  </tam>

  <tam title="Two-Faced Change the Centers" group="Two-Faced"
       formation="Two-Faced Line RH" fractions="3;3;3">
    <path>
      <move select="Run Right"/>
      <move select="Swing Left"/>
      <move select="Run Left" scaleY="2"/>
      <move select="Stand" beats="3"/>
    </path>
    <path>
      <move select="Flip Left"/>
      <move select="Stand" beats="3"/>
      <move select="Dodge Left"/>
      <move select="Swing Left"/>
    </path>
  </tam>

  <tam title="Two-Faced Change the Wave" group="Two-Faced"
       formation="Two-Faced Line RH" fractions="3;3;3">
    <path>
      <move select="Run Right"/>
      <move select="Swing Left"/>
      <move select="Run Left" scaleY="2"/>
      <move select="Flip Left"/>
    </path>
    <path>
      <move select="Flip Left"/>
      <move select="Stand" beats="3"/>
      <move select="Dodge Left"/>
      <move select="Run Right"/>
    </path>
  </tam>

  <tam title="Two-Faced Delight" group="Two-Faced"
       formation="3/4 Lines RH"
       fractions="3;3;3" >
    <path>
      <move select="Quarter Right" offsetY="-1" />
      <move select="Lead Right" beats="4" scaleX="2" scaleY="3" />
      <move select="Lead Right" beats="4" scaleX="3" scaleY="2" />
      <move select="Forward 4" beats="4" />
    </path>
    <path>
      <move select="Quarter Right" offsetY="-1" />
      <move select="Forward 2" beats="4" />
      <move select="Lead Right" beats="4" scaleX="2" scaleY="3" />
      <move select="Lead Right" beats="4" scaleX="3" scaleY="2" />
    </path>
    <path>
      <move select="Flip Left" />
      <move select="Stand" beats="6" />
      <move select="Quarter Right" hands="left" offsetX=".33" offsetY=".33" />
      <move select="Quarter Right" hands="left" offsetX="-.33" offsetY=".33" />
      <move select="Quarter Right" hands="left" offsetX="-.33" offsetY="-.33" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Swing Left" />
      <move select="Swing Left" />
      <move select="Hinge Right" scaleX="2" scaleY="2" offsetX=".33" offsetY=".33" />
      <move select="Hinge Right" scaleX="2" scaleY="2" offsetX="-.33" offsetY=".33" />
      <move select="Hinge Right" scaleX="2" scaleY="2" offsetX="-.33" offsetY="-.33" />
    </path>
  </tam>

  <tam title="Two-Faced Peel Chain Thru" group="Two-Faced"
       formation="3/4 Lines RH">
    <path>
      <move select="Run Right" beats="4" offsetX="-2"/>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Quarter Right" hands="left" offsetX=".33" offsetY=".5" />
      <move select="Quarter Right" hands="left" offsetX="-.17" offsetY=".33" />
      <move select="Quarter Right" hands="left" offsetX="-.33" offsetY=".17" />
    </path>
    <path>
      <move select="Run Left" beats="4" offsetX="-2"/>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="1.5"/>
    </path>
    <path>
      <move select="U-Turn Left" beats="4" offsetY=".5"/>
      <move select="Stand" beats="4"/>
      <move select="Hinge Right" offsetX=".33" offsetY=".5" />
      <move select="Hinge Right" offsetX="-.17" offsetY=".33" />
      <move select="Hinge Right" offsetX=".67" offsetY=".17" />
    </path>
    <path>
      <move select="Run Right" beats="4" scaleY="1.25"/>
      <move select="Swing Left" beats="4" scaleX=".5" scaleY=".5"/>
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="1.5"/>
    </path>
  </tam>

  <tam title="Two-Faced Relay the Deucey" group="Two-Faced"
       formation="Two-Faced Lines RH" fractions="3;4.5;3;3;3">
    <path>
      <move select="Run Right" />
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Flip Left" />
      <move select="Run Right" />
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Flip Left" />
      <move select="Lead Left" beats="4.5" scaleX="3" scaleY="3" />
      <move select="Flip Left" />
      <move select="Run Right" />
      <move select="Swing Left" />
      <move select="Cast Right" />
    </path>
    <path>
      <move select="Flip Left" />
      <move select="Forward 2" beats="4.5" />
      <move select="Forward 2" beats="3" />
      <movement hands="none" beats="3" cx1="1" cy1="0" cx2="3" cy2="2" x2="3" y2="3" />
      <move select="Flip Left" />
      <move select="Cast Right" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Cast Left" />
      <move select="Run Right" />
      <movement hands="none" beats="3" cx1="3" cy1="0" cx2="3" cy2="0" x2="3" y2="-3" />
      <move select="Forward 2" beats="3" />
      <move select="Forward 2" beats="4.5" />
    </path>
  </tam>

  <tam title="Two-Faced Relay the Shadow" group="Two-Faced"
       formation="Tidal Line RH">
    <path>
      <move select="Run Right" scaleY=".5"/>
      <move select="U-Turn Right" hands="left"/>
      <move select="Quarter Right" hands="left" offsetX="-1" offsetY="-.5"/>
      <move select="Stand" beats="1"/>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Hinge Left" beats="2" scaleX=".5" scaleY=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
    </path>
    <path>
      <move select="Flip Left" scaleY=".5"/>
      <move select="Lead Left" beats="4.5" scaleX="3" scaleY="3.5"/>
      <move select="Quarter Left" offsetX="-1" offsetY="1"/>
      <move select="Dodge Left"/>
    </path>
    <path>
      <move select="Run Right" scaleY=".5"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Hinge Left" scaleY=".5"/>
      <move select="Lead Right"/>
      <move select="Dodge Left"/>
    </path>
    <path>
      <move select="Flip Left" scaleY=".5"/>
      <move select="Swing Right"/>
      <move select="Hinge Right" scaleY="1.5"/>
      <move select="Stand" beats="1"/>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Right" beats="2" scaleX="1.5" scaleY="1.5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
    </path>
  </tam>

  <tam title="Two-Faced Relay the Top" group="Two-Faced"
       formation="Two-Faced Lines RH" fractions="3;4.5;3">
    <path>
      <move select="Run Right"/>
      <move select="Cast Left"/>
      <move select="Eighth Right" beats="1.5" offsetX="1.06" offsetY=".06" />
      <move select="Eighth Right" beats="1.5" hands="right" offsetX=".707" offsetY="-.793" />
      <move select="Extend Left" beats="4.5" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Flip Left"/>
      <move select="Lead Left" beats="4.5" scaleX="3" scaleY="3"/>
      <move select="Flip Left"/>
      <move select="Cast Right"/>
    </path>
    <path>
      <move select="Flip Left"/>
      <move select="Extend Left" beats="4.5" scaleX="2" scaleY="2"/>
      <move select="Eighth Left" beats="1.5" hands="left" offsetX=".353" offsetY=".647" />
      <move select="Eighth Left" beats="1.5" hands="left" offsetX=".707" offsetY="-.207" />
      <move select="Cast Right"/>
    </path>
    <path>
      <move select="Run Right"/>
      <move select="Cast Left"/>
      <move select="Run Right"/>
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3"/>
    </path>
  </tam>

  <tam title="Two-Faced Reverse the Top" group="Two-Faced"
       formation="Two-Faced Line RH" fractions="4.5">
    <path>
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3"/>
      <move select="Run Right"/>
    </path>
    <path>
      <move select="Cast Right"/>
      <move select="Flip Left"/>
    </path>
  </tam>

  <tam title="Two-Faced Spin the Top" group="Two-Faced" fractions="3"
       formation="Two-Faced Line RH">
    <path>
      <move select="Run Right"/>
      <move select="Cast Left"/>
    </path>
    <path>
      <move select="Flip Left"/>
      <move select="Lead Left" beats="4.5" scaleX="3" scaleY="3"/>
    </path>
  </tam>

</tamination>
