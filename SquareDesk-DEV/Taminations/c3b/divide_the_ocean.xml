<tamination title="Divide The Ocean | Sea direction | anything">

  <tam title="Divide the Ocean Right" group=" "
       formation="Tidal Wave RH BGGB">
    <path>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Hinge Right" scaleY=".5"/>
      <move select="Run Right" beats="4"/>
      <move select="Forward"/>
    </path>
    <path>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Hinge Right" scaleY=".5"/>
      <move select="Forward 5"/>
    </path>
    <path>
      <move select="Hinge Right" scaleY=".5"/>
      <move select="Lead Right" scaleX=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Right" beats="3"/>
    </path>
    <path>
      <move select="Hinge Right" scaleY=".5"/>
      <move select="Lead Right" scaleX=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Quarter Right" beats="3" offsetX="1" offsetY="1"/>
    </path>
  </tam>

  <tam title="Divide the Sea Out" group=" "
       formation="Tidal Wave RH BGBG">
    <path couples="3 1">
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Hinge Right" scaleY=".5"/>
      <move select="U-Turn Right" beats="5" offsetX="-1"/>
    </path>
    <path couples="3 1">
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Hinge Right" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="3" scaleY="2"/>
      <move select="Forward 2"/>
    </path>
    <path couples="4 2">
      <move select="Hinge Right" scaleY=".5"/>
      <move select="Lead Right" scaleX=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Right" beats="3"/>
    </path>
    <path couples="4 2">
      <move select="Hinge Right" scaleY=".5"/>
      <move select="Lead Right" scaleX=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Left" beats="3"/>
    </path>
  </tam>

</tamination>
