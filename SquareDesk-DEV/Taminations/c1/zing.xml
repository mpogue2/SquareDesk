<tamination title="Zing">

  <tam title="Zing" from="Right-Hand Box" formation="Box RH">
    <path numbers="1 4">
      <move select="Forward 2" beats="2.5"/>
      <move select="Quarter Right" beats="2" offsetX="1" offsetY="1"/>
    </path>
    <path numbers="2 3">
      <move select="Run Left" offsetX="-2"/>
      <move select="Lead Left"/>
    </path>
  </tam>

  <tam title="Zing" from="Left-Hand Box" formation="Box LH">
    <path>
      <move select="Run Right" offsetX="-2"/>
      <move select="Lead Right"/>
    </path>
    <path>
      <move select="Forward 2" beats="2.5"/>
      <move select="Quarter Left" beats="2" offsetX="1" offsetY="-1"/>
    </path>
  </tam>

  <tam title="Zing" from="Right-Hand Columns"
       formation="Column RH GBGB">
    <path>
      <move select="Run Left" offsetX="-1"/>
      <move select="Lead Left" />
    </path>
    <path>
      <move select="Forward" beats="2"/>
      <move select="Quarter Right" beats="2.5" offsetX="1" offsetY="1"/>
    </path>
    <path>
      <move select="Run Left" offsetX="-1"/>
      <move select="Lead Left" />
    </path>
    <path>
      <move select="Forward" beats="2"/>
      <move select="Quarter Right" beats="2.5" offsetX="1" offsetY="1"/>
    </path>
  </tam>

  <tam title="Zing" from="Left-Hand Columns"
       formation="Column LH GBGB">
    <path>
      <move select="Forward" beats="2"/>
      <move select="Quarter Left" beats="2.5" offsetX="1" offsetY="-1"/>
    </path>
    <path>
      <move select="Run Right" offsetX="-1"/>
      <move select="Lead Right" />
    </path>
    <path>
      <move select="Forward" beats="2"/>
      <move select="Quarter Left" beats="2.5" offsetX="1" offsetY="-1"/>
    </path>
    <path>
      <move select="Run Right" offsetX="-1"/>
      <move select="Lead Right" />
    </path>
  </tam>

  <tam title="Zing"
       from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru">
    <path numbers="5 4">
      <move select="Run Right" offsetX="-1"/>
      <move select="Lead Right" />
    </path>
    <path numbers="6 3">
      <move select="Run Left" offsetX="-1"/>
      <move select="Lead Left" />
    </path>
    <path numbers="7 2">
      <move select="Forward" beats="2"/>
      <move select="Quarter Left" beats="2.5" offsetX="1" offsetY="-1"/>
    </path>
    <path numbers="8 1">
      <move select="Forward" beats="2"/>
      <move select="Quarter Right" beats="2.5" offsetX="1" offsetY="1"/>
    </path>
  </tam>

  <tam title="Points Zing" from="Diamonds" formation="Diamonds RH Girl Points">
    <path numbers="6 3">
    </path>
    <path numbers="2 7">
      <move select="Quarter Right" beats="4" offsetX="3"/>
    </path>
    <path numbers="5 4">
    </path>
    <path numbers="8 1">
      <move select="Run Left" offsetX="-2" offsetY="-1"/>
      <move select="Lead Left"/>
    </path>
  </tam>

  <tam title="All 8 Zing" from="" formation="Static MiniWaves RH" group=" ">
    <path>
      <move select="Extend Left" beats="4" scaleX="3" scaleY="2"/>
      <move select="Quarter Right" offsetX="1"/>
    </path>
    <path>
      <move select="Run Left" beats="4" scaleY="1.5" offsetX="-1"/>
      <move select="Forward 2"/>
      <move select="Lead Left"/>
    </path>
    <path>
      <move select="Extend Left" beats="4" scaleX="3" scaleY="2"/>
      <move select="Quarter Right" offsetX="1"/>
    </path>
    <path>
      <move select="Run Left" beats="4" scaleY="1.5" offsetX="-1"/>
      <move select="Forward 2"/>
      <move select="Lead Left"/>
    </path>
  </tam>

  <tam title="As Couples Zing" from="" formation="Two-Faced Lines RH" group=" ">
    <path>
      <move select="Extend Right" beats="4" hands="right" scaleX="2"/>
      <move select="Quarter Right" beats="4" hands="gripright" offsetX="3" offsetY="1"/>
    </path>
    <path>
      <move select="Forward 2" hands="left" beats="4"/>
      <move select="Quarter Right" beats="4" hands="gripleft" offsetX="1" offsetY="2"/>
    </path>
    <path>
      <move select="Run Left" hands="left" scaleY="1.5"/>
      <move select="Forward 3" hands="left"/>
      <move select="Lead Left" beats="2" scaleX="2" hands="gripleft"/>
    </path>
    <path>
      <move select="U-Turn Left" hands="right"/>
      <move select="Forward 3" hands="right"/>
      <move select="Quarter Left" beats="2" hands="gripright"/>
    </path>
  </tam>

  <tam title="Butterfly Zing" from="" formation="Butterfly RH" group=" ">
    <path>
      <move select="Lead Right" beats="3" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Run Left" scaleX=".5" scaleY=".5" offsetX="-1"/>
      <move select="Lead Left" beats="3" scaleY="3"/>
    </path>
    <path>
      <move select="Run Left"/>
      <move select="Quarter Left" beats="2" offsetX="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2"/>
      <move select="Quarter Right"/>
    </path>
  </tam>

  <tam title="Concentric Zing" from="" formation="Column RH GBGB" group=" ">
    <path>
      <move select="Run Left" scaleY="1.5" offsetX="-1"/>
      <move select="Forward 2"/>
      <move select="Lead Left"/>
    </path>
    <path>
      <move select="Run Left" scaleX=".5" scaleY=".5" offsetX="-1"/>
      <move select="Lead Left"/>
    </path>
    <path>
      <move select="Quarter Right" beats="3" offsetX="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2"/>
      <move select="Quarter Right" beats="2" offsetX="2"/>
    </path>
  </tam>

  <tam title="&quot;O&quot; Zing" from="" formation="O RH" group=" ">
    <path>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2"/>
      <move select="Quarter Right"/>
    </path>
    <path>
      <move select="Run Left"/>
      <move select="Quarter Left" beats="2" offsetX="2"/>
    </path>
    <path>
      <move select="Run Left" scaleX=".5" scaleY=".5" offsetX="-1"/>
      <move select="Lead Left" beats="3" scaleY="3"/>
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX="2" scaleY="2"/>
    </path>
  </tam>

  <tam title="Stretch Zing" from="Right-Hand Columns" group=" "
       formation="Column RH GBGB">
    <path>
      <move select="Run Left" offsetX="-1"/>
      <move select="Forward 2"/>
      <move select="Lead Left" />
    </path>
    <path>
      <move select="Forward" beats="2"/>
      <move select="Quarter Right" beats="2.5" offsetX="1" offsetY="1"/>
    </path>
    <path>
      <move select="Run Left" offsetX="-1"/>
      <move select="Lead Left" />
    </path>
    <path>
      <move select="Forward 3"/>
      <move select="Quarter Right" beats="2.5" offsetX="1" offsetY="1"/>
    </path>
  </tam>

  <tam title="Tandem Zing" from="" formation="Column RH GBGB" group=" ">
    <path>
      <move select="Run Left" offsetX="-1"/>
      <move select="Forward 2"/>
      <move select="Lead Left" beats="2" scaleY="2"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Run Left" offsetX="-1"/>
      <move select="Forward 2"/>
      <move select="Quarter Left" offsetX="1"/>
    </path>
    <path>
      <move select="Forward 2" beats="6"/>
      <move select="Quarter Right"/>
    </path>
    <path>
      <move select="Extend Left" beats="6" scaleX="3"/>
      <move select="Quarter Right" beats="2" offsetX="1" offsetY="1"/>
    </path>
  </tam>

</tamination>
