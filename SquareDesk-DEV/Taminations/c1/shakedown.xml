<tamination title="Shakedown">

  <tam title="Shakedown" from="Couples Facing Out" formation="Couples Facing Out Compact">
    <path numbers="1 4" couples="3 1">
      <move select="U-Turn Right" offsetX="-2" offsetY="-1" />
      <move select="Quarter Right" offsetX=".5" />
    </path>
    <path numbers="2 3" couples="3 1">
      <move select="Flip Right" scaleY="1.5" />
      <move select="Quarter Right" offsetX=".5" />
    </path>
  </tam>

  <tam title="Shakedown" from="Trade By" formation="Trade By">
    <path>
      <move select="U-Turn Right" offsetX="-1" offsetY="-1" />
      <move select="Quarter Right" offsetX="1" />
    </path>
    <path>
      <move select="Flip Right" />
      <move select="Quarter Right" offsetY="1" />
    </path>
    <path>
      <move select="U-Turn Right" offsetX="-1" offsetY="-1" />
      <move select="Quarter Right" offsetX="1" />
    </path>
    <path>
      <move select="Flip Right" />
      <move select="Quarter Right" offsetY="1" />
    </path>
  </tam>

  <tam title="Shakedown"
       from="Lines Facing Out"
       formation="Lines Facing Out Compact">
    <path>
      <move select="U-Turn Right" offsetX="-1.5" offsetY="-1" />
      <move select="Lead Right" />
    </path>
    <path>
      <move select="Flip Right" />
      <move select="Quarter Right" offsetX=".5" />
    </path>
    <path>
      <move select="U-Turn Right" offsetX="-1.5" offsetY="-.5" />
      <move select="Lead Right" scaleY=".5" />
    </path>
    <path>
      <move select="Flip Right" />
      <move select="Quarter Right" offsetX=".5" />
    </path>
  </tam>

  <tam title="Left Shakedown"
       from="Trade By"
       formation="Trade By">
    <path>
      <move select="Flip Left" />
      <move select="Quarter Left" offsetY="-1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1" offsetY="1" />
      <move select="Quarter Left" offsetX="1" />
    </path>
    <path>
      <move select="Flip Left" />
      <move select="Quarter Left" offsetY="-1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1" offsetY="1" />
      <move select="Quarter Left" offsetX="1" />
    </path>
  </tam>

  <tam title="Left Shakedown"
       from="Lines Facing Out"
       formation="Lines Facing Out Compact">
    <path>
      <move select="Flip Left" />
      <move select="Quarter Left" offsetX=".5" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1.5" offsetY=".5" />
      <move select="Lead Left" scaleY=".5" />
    </path>
    <path>
      <move select="Flip Left" />
      <move select="Quarter Left" offsetX=".5" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1.5" offsetY="1" />
      <move select="Lead Left" />
    </path>
  </tam>

  <tam title="All 8 Shakedown" from="" formation="Static Facing Out" group=" ">
    <path>
      <move select="Flip Right" beats="5" scaleY="2" />
      <move select="Quarter Right" beats="3" offsetX="2" />
    </path>
    <path>
      <move select="U-Turn Right" beats="5" offsetX="-2" offsetY="-2" />
      <move select="Quarter Right" beats="3" offsetX="2" />
    </path>
    <path>
      <move select="Flip Right" beats="5" scaleY="2"/>
      <move select="Quarter Right" beats="3" offsetX="2" />
    </path>
    <path>
      <move select="U-Turn Right" beats="5" offsetX="-2" offsetY="-2" />
      <move select="Quarter Right" beats="3" offsetX="2" />
    </path>
  </tam>

  <tam title="As Couples Shakedown"
       from="Lines Facing Out"
       formation="Lines Facing Out Compact"
       group=" " >
    <path>
      <move select="Beau Wheel" reflect="-1" offsetX="-2" offsetY="-.5" />
      <move select="Quarter Right" hands="left" offsetX=".5" />
    </path>
    <path>
      <move select="Belle Wheel" reflect="-1" offsetX="-2" offsetY="-.5" />
      <move select="Hinge Right" scaleX="2" scaleY="2" offsetX=".5" />
    </path>
    <path>
      <move select="Run Right" hands="left" offsetX="1.5" />
      <move select="Quarter Right" hands="left" offsetY=".5" />
    </path>
    <path>
      <move select="Run Right" hands="right" scaleX="3" scaleY="3" offsetX="1.5" />
      <move select="Hinge Right" scaleX="2" scaleY="2" offsetY=".5" />
    </path>
  </tam>

  <tam title="Butterfly Shakedown" from="" formation="Butterfly Trade By" group=" ">
    <path>
      <move select="Lead Right" beats="3" scaleY="3"/>
      <move select="Flip Right" scaleY=".5" offsetX="3"/>
    </path>
    <path>
      <move select="U-Turn Right" offsetX="-1"/>
      <move select="Lead Right" beats="2" scaleY="2"/>
    </path>
    <path>
      <move select="Quarter Right" offsetY="-1"/>
      <move select="Quarter Right" offsetX="1"/>
      <move select="Quarter Right"/>
    </path>
    <path>
      <move select="U-Turn Right" offsetX="-1" offsetY="-2"/>
      <move select="Quarter Right" offsetX="1"/>
    </path>
  </tam>

  <tam title="Concentric Shakedown"
       from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru"
       group=" " >
    <path>
      <move select="U-Turn Right" offsetX="-2" offsetY="-2" />
      <move select="Quarter Right" offsetX="2" />
    </path>
    <path>
      <move select="Flip Right" offsetX="-1" offsetY="-1" />
      <move select="Quarter Right" offsetX="1" offsetY="1" />
    </path>
    <path>
      <move select="U-Turn Right" offsetX="-1"/>
      <move select="Quarter Right" offsetX="1" />
    </path>
    <path>
      <move select="Flip Right" scaleY=".67" />
      <move select="Quarter Right" offsetY=".67" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Shakedown" from="" formation="O Trade By" group=" ">
    <path>
      <move select="Quarter Right" offsetY="-1"/>
      <move select="Quarter Right" offsetX="1"/>
      <move select="Quarter Right"/>
    </path>
    <path>
      <move select="U-Turn Right" offsetX="-1" offsetY="-2"/>
      <move select="Quarter Right" offsetX="1"/>
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX=".5" scaleY="3"/>
      <move select="Flip Right" scaleY=".25" offsetX="3"/>
    </path>
    <path>
      <move select="U-Turn Right" offsetX="-1"/>
      <move select="Lead Right" beats="2" scaleY="2"/>
    </path>
  </tam>

  <tam title="Stretch Shakedown (from columns)"
       from="Trade By"
       formation="Trade By"
       group=" " >
    <path>
      <move select="U-Turn Right" offsetX="-2" offsetY="-1" />
      <move select="Quarter Right" offsetX="2" />
    </path>
    <path>
      <move select="Flip Right" />
      <move select="Quarter Right" offsetY="1" />
    </path>
    <path>
      <move select="U-Turn Right" offsetX="-1" offsetY="-1" />
      <move select="Quarter Right" offsetX="1" />
    </path>
    <path>
      <move select="Flip Right" offsetX="1" />
      <move select="Quarter Right" offsetX="-1" offsetY="1" />
    </path>
  </tam>

  <tam title="Stretch Shakedown (from lines)"
       from="Lines Facing Out"
       formation="Lines Facing Out Compact"
       group=" " >
    <path>
      <move select="U-Turn Right" offsetX="-1" offsetY="-1" />
      <move select="Lead Right" scaleX="1.5" />
    </path>
    <path>
      <move select="Flip Right" />
      <move select="Quarter Right" offsetX=".5" />
    </path>
    <path>
      <move select="U-Turn Right" offsetX="-1" offsetY="-2.5" />
      <move select="Lead Right" scaleX="1.5" scaleY=".5" />
    </path>
    <path>
      <move select="Flip Right" scaleY="2" />
      <move select="Quarter Right" offsetX=".5" />
    </path>
  </tam>


  <tam title="As Couples Left Shakedown"
       from="Lines Facing Out"
       formation="Lines Facing Out Compact"
       group=" ">
    <path>
      <move select="Run Left" hands="left" scaleX="3" scaleY="3" offsetX="1.5" />
      <move select="Hinge Left" scaleX="2" scaleY="2" offsetY="-.5" />
    </path>
    <path>
      <move select="Run Left" hands="right" offsetX="1.5" />
      <move select="Quarter Left" hands="right" offsetY="-.5" />
    </path>
    <path>
      <move select="Belle Wheel" offsetX="-2" offsetY=".5" />
      <move select="Hinge Left" scaleX="2" scaleY="2" offsetX=".5" />
    </path>
    <path>
      <move select="Beau Wheel" offsetX="-2" offsetY=".5" />
      <move select="Quarter Left" hands="right" offsetX=".5" />
    </path>
  </tam>

  <tam title="Concentric Left Shakedown"
       from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru"
       group=" ">
    <path>
      <move select="Flip Left" offsetX="-1" offsetY="1" />
      <move select="Quarter Left" offsetX="1" offsetY="-1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-2" offsetY="2" />
      <move select="Quarter Left" offsetX="2" />
    </path>
    <path>
      <move select="Flip Left" scaleY=".67" />
      <move select="Quarter Left" offsetY="-.67" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1" />
      <move select="Quarter Left" offsetX="1" />
    </path>
  </tam>

  <tam title="Stretch Left Shakedown (from columns)"
       from="Trade By"
       formation="Trade By"
       group=" ">
    <path>
      <move select="Flip Left" />
      <move select="Quarter Left" offsetY="-1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-2" offsetY="1" />
      <move select="Quarter Left" offsetX="2" />
    </path>
    <path>
      <move select="Flip Left" offsetX="1" />
      <move select="Quarter Left" offsetX="-1" offsetY="-1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1" offsetY="1" />
      <move select="Quarter Left" offsetX="1" />
    </path>
  </tam>

  <tam title="Stretch Left Shakedown (from lines)"
       from="Lines Facing Out"
       formation="Lines Facing Out Compact"
       group=" ">
    <path>
      <move select="Flip Left" scaleY="2" />
      <move select="Quarter Left" offsetX=".5" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1" offsetY="2.5" />
      <move select="Lead Left" scaleX="1.5" scaleY=".5" />
    </path>
    <path>
      <move select="Flip Left" />
      <move select="Quarter Left" offsetX=".5" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1" offsetY="1" />
      <move select="Lead Left" scaleX="1.5" />
    </path>
  </tam>


</tamination>
