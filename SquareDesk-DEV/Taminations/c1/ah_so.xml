<tamination title="Ah So">

  <tam title="Ah So" from="Right-Hand Wave" formation="Wave RH">
    <path numbers="1 4">
      <move select="Run Right" hands="gripright" scaleX="1.5" scaleY="2" offsetX="1.5" />
    </path>
    <path numbers="2 3">
      <move select="U-Turn Right" hands="gripright" offsetX="-1.5" />
    </path>
  </tam>

  <tam title="Ah So" from="Left-Hand Wave" formation="Wave LH">
    <path>
      <move select="Run Left" hands="gripleft" scaleX="1.5" scaleY="2" offsetX="1.5" />
    </path>
    <path>
      <move select="U-Turn Left" hands="gripleft" offsetX="-1.5" />
    </path>
  </tam>

  <tam title="Ah So"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB">
    <path>
      <move select="Run Right" hands="gripright" scaleY="2" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Right" hands="gripright" offsetX="-1" />
    </path>
    <path>
      <move select="U-Turn Right" hands="gripright" offsetX="-1" />
    </path>
    <path>
      <move select="Run Right" hands="gripright" scaleY="2" offsetX="1" />
    </path>
  </tam>

  <tam title="Ah So"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB">
    <path>
      <move select="Run Left" hands="gripleft" scaleY="2" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Left" hands="gripleft" offsetX="-1" />
    </path>
    <path>
      <move select="U-Turn Left" hands="gripleft" offsetX="-1" />
    </path>
    <path>
      <move select="Run Left" hands="gripleft" scaleY="2" offsetX="1" />
    </path>
  </tam>

  <tam title="Ah So"
       from="Inverted Lines"
       formation="Inverted Lines Ends Facing Out">
    <path>
      <move select="Run Left" hands="gripleft" scaleY="2" offsetX="-1" />
    </path>
    <path>
      <move select="U-Turn Left" hands="gripleft" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Right" hands="gripright" offsetX="-1" />
    </path>
    <path>
      <move select="Run Right" hands="gripright" scaleY="2" offsetX="1" />
    </path>
  </tam>

  <tam title="Ah So" from="3 and 1 Lines #1" formation="3 and 1 lines #1">
    <path>
      <move select="Run Left" beats="4" hands="left" scaleY="2" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Left" beats="4" hands="left" offsetX="-1" />
    </path>
    <path>
      <move select="U-Turn Left" beats="4" hands="gripright" offsetX="1" />
    </path>
    <path>
      <move select="Run Left" beats="4" hands="gripleft" scaleY="2" offsetX="1" />
    </path>
  </tam>

  <tam title="Ah So" from="3 and 1 Lines #2" formation="3 and 1 lines #2">
    <path>
      <move select="Run Right" beats="4" hands="right" scaleY="2" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="right" offsetX="-1" />
    </path>
    <path>
      <move select="U-Turn Left" beats="4" hands="gripright" offsetX="-1" />
    </path>
    <path>
      <move select="Run Left" beats="4" hands="gripleft" scaleY="2" offsetX="-1" />
    </path>
  </tam>

  <tam title="Ah So" from="3 and 1 Lines #3" formation="3 and 1 lines #3">
    <path>
      <move select="Run Right" beats="4" hands="right" scaleY="2" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="left" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Left" beats="4" hands="gripleft" offsetX="1" />
    </path>
    <path>
      <move select="Run Left" beats="4" hands="gripleft" scaleY="2" offsetX="-1" />
    </path>
  </tam>

  <tam title="Ah So" from="3 and 1 Lines #4" formation="3 and 1 lines #4">
    <path>
      <move select="Run Right" beats="4" hands="right" scaleY="2" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="left" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="-1" />
    </path>
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleY="2" offsetX="1" />
    </path>
  </tam>

  <tam title="Ah So" from="3 and 1 Lines #5" formation="3 and 1 lines #5">
    <path>
      <move select="Run Right" beats="4" hands="right" scaleY="2" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="right" offsetX="-1" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="gripleft" offsetX="1" />
    </path>
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleY="2" offsetX="1" />
    </path>
  </tam>

  <tam title="Ah So" from="3 and 1 Lines #6">
     <formation>
      <dancer gender="girl" x="-2" y="3" angle="180"/>
      <dancer gender="boy" x="-2" y="1" angle="0"/>
      <dancer gender="girl" x="-2" y="-1" angle="180"/>
      <dancer gender="boy" x="-2" y="-3" angle="180"/>
    </formation>
    <path numbers="5 4">
      <move select="Run Left" beats="4" hands="left" scaleY="2" offsetX="-1" />
    </path>
    <path numbers="6 3">
      <move select="U-Turn Left" beats="4" hands="left" offsetX="1" />
    </path>
    <path numbers="7 2">
      <move select="U-Turn Right" beats="4" hands="left" offsetX="1" />
    </path>
    <path numbers="8 1">
      <move select="Run Right" beats="4" hands="right" scaleX="2" scaleY="2" offsetX="1" />
    </path>
  </tam>

  <tam title="Ah So" from="3 and 1 Lines #7" formation="3 and 1 lines #7">
    <path>
      <move select="Run Left" beats="4" hands="left" scaleY="2" offsetX="-1" />
    </path>
    <path>
      <move select="U-Turn Left" beats="4" hands="right" offsetX="-1" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="-1" />
    </path>
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleY="2" offsetX="1" />
    </path>
  </tam>

  <tam title="Ah So" from="3 and 1 Lines #8" formation="3 and 1 lines #8">
    <path>
      <move select="Run Left" beats="4" hands="left" scaleY="2" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Left" beats="4" hands="right" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Left" beats="4" hands="gripleft" offsetX="-1" />
    </path>
    <path>
      <move select="Run Left" beats="4" hands="gripleft" scaleY="2" offsetX="1" />
    </path>
  </tam>

  <tam title="As Couples Ah So"
       from="Two-Faced Tidal Line"
       formation="Two-Faced Tidal Line RH"
       group=" ">
    <path>
      <move select="Run Right" beats="5" hands="gripright" scaleX="2" scaleY="3" offsetX="1.5" offsetY="-.5" />
    </path>
    <path>
      <move select="Run Right" beats="5" hands="gripboth" offsetX="1.5" offsetY=".5" scaleY="2" />
    </path>
    <path>
      <move select="U-Turn Right" beats="5" hands="gripboth" offsetX="-1.5" offsetY=".5" />
    </path>
    <path>
      <move select="Run Right" beats="5" hands="gripright" scaleY="3" offsetX="-1.5" offsetY="3.5" />
    </path>
  </tam>

  <tam title="Concentric Ah So" from="Tidal Wave" formation="Tidal Wave RH BGGB" group=" ">
    <path>
      <move select="Run Right" hands="gripright" offsetX="3" scaleY="2.25"/>
    </path>
    <path>
      <move select="U-Turn Right" hands="gripright" offsetX="-3" offsetY="1.5"/>
    </path>
    <path>
      <move select="Run Right" hands="gripright" scaleY="1.25" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Right" hands="gripright" offsetX="-1" offsetY="-.5"/>
    </path>
  </tam>

  <tam title="Stretch Ah So" from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB Compact" group=" ">
    <path>
      <move select="Run Right" hands="gripright" scaleY="2" offsetX="2.5" />
    </path>
    <path>
      <move select="U-Turn Right" hands="gripright" offsetX="-2.5" />
    </path>
    <path>
      <move select="U-Turn Right" hands="gripright" offsetX="-1.5" />
    </path>
    <path>
      <move select="Run Right" hands="gripright" scaleY="2" offsetX="1.5" />
    </path>
  </tam>

  <tam title="Tandem Ah So" from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB Compact" group=" ">
    <path>
      <move select="Counter Rotate Right 2 0" hands="gripright" beats="4" offsetX="2"/>
      <move select="Counter Rotate Right 2 0" hands="gripright" beats="4" offsetX="2" offsetY=".5"/>
    </path>
    <path>
      <move select="Counter Rotate Right -1 -3" hands="gripright" beats="4" offsetX="-1" offsetY="1"/>
      <move select="Counter Rotate Right -1 -3" hands="gripright" beats="4" offsetX="-1" offsetY=".5"/>
    </path>
    <path>
      <move select="U-Turn Right" hands="gripright" beats="8" offsetX=".5"/>
    </path>
    <path>
      <move select="Run Right" hands="gripright" beats="8" offsetX="-.5" scaleX="1.5" scaleY="2"/>
    </path>
  </tam>


</tamination>
