<tamination title="Tandem Concept">

  <tam title="Tandem Bend the Line" group="Tandem"
       formation="Two-Faced Lines RH" >
    <path>
      <move select="Counter Rotate Right 2 0" beats="3" hands="right" offsetX="1"/>
    </path>
    <path>
      <move select="Counter Rotate Right 0 2" beats="3" hands="left" offsetX="1" />
    </path>
    <path>
      <move select="Counter Rotate Right -2 0" beats="3" hands="left" offsetX="-1" />
    </path>
    <path>
      <move select="Counter Rotate Right 0 -2" beats="3" hands="right" offsetX="-1" />
    </path>
  </tam>

  <tam title="Tandem Box Circulate" group="Tandem"
       from=""
       formation="Column RH GBGB"
       fractions="" >
    <path>
      <move select="Run Right" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Forward 2" />
      <move select="Run Right" />
    </path>
    <path>
      <move select="Forward 4" beats="5" />
    </path>
    <path>
      <move select="Forward 4" beats="5" />
    </path>
  </tam>

  <tam title="Tandem Box Transfer" group="Tandem"
       formation="Column RH GBGB">
    <path>
      <move select="Counter Rotate Right 2 -4" beats="3.333"/>
      <move select="Counter Rotate Right 2 -4" beats="3.333"/>
      <move select="Counter Rotate Right 2 -4" beats="3.333"/>
    </path>
    <path>
      <move select="Counter Rotate Right 4 -2" beats="3.333"/>
      <move select="Counter Rotate Right 4 -2" beats="3.333"/>
      <move select="Counter Rotate Right 4 -2" beats="3.333"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Counter Rotate Right 0 -2" hands="right"/>
      <move select="Counter Rotate Right 0 -2" hands="right"/>
      <move select="Counter Rotate Right 0 -2" hands="right"/>
      <move select="Forward 2"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Counter Rotate Right 2 0" hands="right"/>
      <move select="Counter Rotate Right 2 0" hands="right"/>
      <move select="Counter Rotate Right 2 0" hands="right"/>
      <move select="Forward 2"/>
    </path>
  </tam>

  <tam title="Tandem Cast Off 3/4" group="Tandem"
       from=""
       formation="Column RH GBGB"
       fractions="" >
    <path>
      <move select="Counter Rotate Right 0 -2" hands="right"/>
      <move select="Counter Rotate Right 0 -2" hands="right"/>
      <move select="Counter Rotate Right 0 -2" offsetY="-1" hands="right"/>
    </path>
    <path>
      <move select="Counter Rotate Right 2 0" hands="right"/>
      <move select="Counter Rotate Right 2 0" hands="right"/>
      <move select="Counter Rotate Right 2 0" offsetY="1" hands="right"/>
    </path>
    <path>
      <move select="Counter Rotate Right 0 -2" hands="right"/>
      <move select="Counter Rotate Right 0 -2" hands="right"/>
      <move select="Counter Rotate Right 0 -2" offsetY="-1" hands="right"/>
    </path>
    <path>
      <move select="Counter Rotate Right 2 0" hands="right"/>
      <move select="Counter Rotate Right 2 0" hands="right"/>
      <move select="Counter Rotate Right 2 0" offsetY="1" hands="right"/>
    </path>
  </tam>

  <tam title="Tandem Centers Run" group="Tandem"
       from=""
       formation="Ocean Waves RH BGGB"
       fractions="" >
    <path>
      <move select="Dodge Right" beats="6" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Forward 4" beats="3" />
    </path>
    <path>
      <move select="Forward 4" beats="3" />
      <move select="Run Right" />
    </path>
    <path>
      <move select="Dodge Right" beats="6" />
    </path>
  </tam>

  <tam title="Tandem Diamond Circulate" group="Tandem"
       formation="T-Bone ULLD">
    <path>
      <move select="Forward" beats="1.5"/>
      <move select="Lead Right" beats="2.5" scaleY="2"/>
    </path>
    <path>
      <move select="Forward"/>
      <move select="Lead Right" beats="3" scaleY="2"/>
    </path>
    <path>
      <move select="Forward 3"/>
      <move select="Quarter Right" offsetX="1"/>
    </path>
    <path>
      <move select="Quarter Right" offsetY="-1"/>
      <move select="Forward 3"/>
    </path>
    <taminator>
    If you have trouble visualizing the diamond, look at the center
    point of each tandem.
    </taminator>
  </tam>

  <tam title="Tandem Dixie Style to a Wave" group="Tandem" formation="Double Pass Thru">
    <path>
      <move select="Back" beats="2"/>
      <move select="Dodge Right"/>
      <move select="Forward" beats="2"/>
      <move select="Counter Rotate Left 2 0" hands="left" offsetY="-1"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="1.5" scaleY="1.5"/>
      <move select="Forward 2" beats="3"/>
      <move select="Extend Right" beats="2" scaleX=".5"/>
      <move select="Counter Rotate Left 2 0" hands="left" offsetY="-1.5"/>
    </path>
    <path>
      <move select="Back 2"/>
      <move select="Dodge Right"/>
      <move select="Forward 2"/>
      <move select="Counter Rotate Left 0 2" hands="left" offsetY="1"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="1.5"/>
      <move select="Forward 2" beats="3"/>
      <move select="Extend Right" beats="2"/>
      <move select="Counter Rotate Left 0 2" hands="left" offsetY=".5"/>
    </path>
  </tam>

  <tam title="Tandem Fan the Top" group="Tandem" formation="Ocean Waves RH BGGB">
    <path>
      <move select="Counter Rotate Right 5 -1" beats="9"/>
    </path>
    <path>
      <move select="Counter Rotate Left -1 3" beats="3"/>
      <move select="Counter Rotate Left -1 3" beats="3"/>
      <move select="Counter Rotate Left -1 3" beats="3"/>
    </path>
    <path>
      <move select="Counter Rotate Left 3 -1" beats="3"/>
      <move select="Counter Rotate Left 3 -1" beats="3"/>
      <move select="Counter Rotate Left 3 -1" beats="3"/>
    </path>
    <path>
      <move select="Counter Rotate Right 1 -5" beats="9"/>
    </path>
  </tam>

  <tam title="Tandem Hinge" group="Tandem" formation="Column RH GBGB">
    <path>
      <move select="Counter Rotate Right 0 -2" offsetY="-1" hands="right"/>
    </path>
    <path>
      <move select="Counter Rotate Right 2 0" offsetY="1" hands="right"/>
    </path>
    <path>
      <move select="Counter Rotate Right 0 -2" offsetY="-1" hands="right"/>
    </path>
    <path>
      <move select="Counter Rotate Right 2 0" offsetY="1" hands="right"/>
    </path>
  </tam>

  <tam title="Tandem Pass the Ocean" group="Tandem"
       from=""
       formation="Double Pass Thru"
       fractions="" >
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward 4" />
      <move select="Quarter Right" offsetX="1" offsetY=".5" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward 2" />
      <move select="Quarter Left" beats="4" offsetX="1" offsetY="-1.5" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward" />
      <move select="Lead Right" beats="4" scaleX="2" scaleY="3.5" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward" />
      <move select="Quarter Left" beats="4" offsetY="2.5" />
    </path>
  </tam>

  <tam title="Tandem Quarter Thru" group="Tandem" fractions="2">
     <formation>
      <dancer gender="girl" x="-1" y="3" angle="90"/>
      <dancer gender="boy" x="-1" y="1" angle="90"/>
      <dancer gender="girl" x="-1" y="-1" angle="90"/>
      <dancer gender="boy" x="-1" y="-3" angle="90"/>
    </formation>
    <path numbers="1 8">
      <move select="Counter Rotate Right 0 -2" hands="right" offsetY="-1"/>
    </path>
    <path numbers="2 7">
      <move select="Counter Rotate Right 2 0" hands="right" offsetY="1"/>
    </path>
    <path numbers="3 6">
      <move select="Counter Rotate Right 0 -2" hands="right" offsetY="-1"/>
      <move select="Counter Rotate Left 0 2" hands="left" offsetX="-1"/>
      <move select="Counter Rotate Left 0 2" hands="left" offsetY="1"/>
    </path>
    <path numbers="4 5">
      <move select="Counter Rotate Right 2 0" hands="right" offsetY="1"/>
      <move select="Counter Rotate Left 2 0" hands="left" offsetX="1"/>
      <move select="Counter Rotate Left 2 0" hands="left" offsetY="-1"/>
    </path>
  </tam>

  <tam title="Tandem Right Roll to a Wave" group="Tandem"
       formation="Column LH GBGB">
    <path>
      <move select="Forward" beats="2"/>
    </path>
    <path>
      <move select="Forward 3" beats="4"/>
    </path>
    <path>
      <move select="Forward" beats="2"/>
      <move select="Run Right"/>
    </path>
    <path>
      <move select="Run Right" offsetX="-1"/>
      <move select="Forward 4"/>
    </path>
  </tam>

  <tam title="Tandem Scoot Back" group="Tandem"
       formation="Column RH GBGB">
    <path>
      <move select="Run Right" beats="5" />
      <move select="Forward 2" beats="3" />
    </path>
    <path>
      <move select="Forward 2" beats="3" />
      <move select="Run Right" beats="5" />
    </path>
    <path>
      <move select="Forward 2" />
      <move select="Counter Rotate Right 0 -2" hands="right"/>
      <move select="Counter Rotate Right 0 -2" hands="right"/>
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Forward 2" />
      <move select="Counter Rotate Right 2 0" hands="right"/>
      <move select="Counter Rotate Right 2 0" hands="right"/>
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="Tandem Single Wheel" group="Tandem"
       formation="Completed Double Pass Thru">
    <path>
      <move select="Flip Left" offsetX="-1" scaleY=".5"/>
      <move select="Forward 2" scaleX="0.6"  />
    </path>
    <path>
      <move select="Flip Right" beats="5" scaleY=".5" offsetX=".4"/>
    </path>
    <path>
      <move select="Forward" beats="2" />
      <move select="Flip Left"  scaleY=".5"/>
    </path>
    <path>
      <move select="Forward 3"/>
      <move select="Flip Right" scaleY=".5" offsetX=".7"/>
    </path>
  </tam>

  <tam title="Tandem Square Chain Thru" group="Tandem"
       formation="Double Pass Thru" fractions="5;4;4">
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="1.5"/>
      <move select="Counter Rotate Right 2 0" offsetY="-1.5" beats="3"/>
      <move select="Counter Rotate Left 2 0" hands="left"/>
      <move select="Counter Rotate Left 2 0" hands="left"/>
      <move select="Stand" beats="4"/>
      <move select="Counter Rotate Left 2 0" hands="left"/>
      <move select="Counter Rotate Left 2 0" hands="left"/>
      <move select="Forward 2" beats="3"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Counter Rotate Left 2 0" offsetX="2" beats="3"/>
      <move select="Counter Rotate Left 2 0" hands="left"/>
      <move select="Counter Rotate Left 2 0" hands="left"/>
      <move select="Counter Rotate Right 2 0" hands="right"/>
      <move select="Counter Rotate Right 2 0" hands="right"/>
      <move select="Counter Rotate Left 2 0" hands="left"/>
      <move select="Counter Rotate Left 2 0" hands="left"/>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="1.5"/>
      <move select="Counter Rotate Right 0 -2" offsetY="-1.5" beats="3"/>
      <move select="Counter Rotate Left 0 2" hands="left"/>
      <move select="Counter Rotate Left 0 2" hands="left"/>
      <move select="Stand" beats="4"/>
      <move select="Counter Rotate Left 0 2" hands="left"/>
      <move select="Counter Rotate Left 0 2" hands="left"/>
      <move select="Forward 2" beats="3"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Counter Rotate Left 0 2" offsetX="2" beats="3"/>
      <move select="Counter Rotate Left 0 2" hands="left"/>
      <move select="Counter Rotate Left 0 2" hands="left"/>
      <move select="Counter Rotate Right 0 -2" hands="right"/>
      <move select="Counter Rotate Right 0 -2" hands="right"/>
      <move select="Counter Rotate Left 0 2" hands="left"/>
      <move select="Counter Rotate Left 0 2" hands="left"/>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2"/>
    </path>
  </tam>

  <tam title="Tandem Swing Thru" group="Tandem"
       formation="Ocean Waves RH BGGB"
       fractions="4" >
    <path numbers="2 7">
      <move select="Counter Rotate Right 2 0" hands="right" offsetX="1"/>
      <move select="Counter Rotate Right 2 0" hands="right" offsetY="1"/>
      <move select="Counter Rotate Left 2 0" hands="left" offsetX="1"/>
      <move select="Counter Rotate Left 2 0" hands="left" offsetY="-1"/>
    </path>
    <path numbers="4 5">
      <move select="Counter Rotate Right 0 -2" hands="right" offsetX="-1"/>
      <move select="Counter Rotate Right 0 -2" hands="right" offsetY="-1"/>
    </path>
    <path numbers="6 3">
      <move select="Counter Rotate Right 2 0" hands="right" offsetX="1"/>
      <move select="Counter Rotate Right 2 0" hands="right" offsetY="1"/>
    </path>
    <path numbers="8 1">
      <move select="Counter Rotate Right 0 -2" hands="right" offsetX="-1"/>
      <move select="Counter Rotate Right 0 -2" hands="right" offsetY="-1"/>
      <move select="Counter Rotate Left 0 2" hands="left" offsetX="-1"/>
      <move select="Counter Rotate Left 0 2" hands="left" offsetY="1"/>
    </path>
  </tam>

  <tam title="Tandem Touch 1/4" group="Tandem" formation="Double Pass Thru">
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2"/>
      <move select="Counter Rotate Right 2 0" hands="right"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Counter Rotate Right 2 0" hands="right"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2"/>
      <move select="Counter Rotate Right 0 -2" hands="right"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Counter Rotate Right 0 -2" hands="right"/>
    </path>
  </tam>

  <tam title="Tandem Walk and Dodge" group="Tandem" formation="Column RH GBGB">
    <path>
      <move select="Dodge Right" beats="4"/>
    </path>
    <path>
      <move select="Dodge Right" beats="4"/>
    </path>
    <path>
      <move select="Forward 4"/>
    </path>
    <path>
      <move select="Forward 4"/>
    </path>
  </tam>

  <tam title="Tandem Zoom" group="Tandem"
       from=""
       formation="Column RH GBGB"
       fractions="" >
    <path>
      <move select="Flip Left" beats="2" scaleX=".75" scaleY=".75" />
      <move select="Forward 6" />
      <move select="Flip Left" beats="2" scaleX=".75" scaleY=".75" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Forward 2" />
      <move select="Flip Left" beats="2" scaleX=".75" scaleY=".75" />
      <move select="Forward 6" />
      <move select="Flip Left" beats="2" scaleX=".75" scaleY=".75" />
    </path>
    <path>
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Forward 4" />
    </path>
  </tam>

