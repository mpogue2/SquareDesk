<tamination title="O Formation / Concept">

<tam title="&quot;O&quot; Chase Right" group="&quot;O&quot;"
       formation="O Trade By">
    <path>
      <move select="Run Right" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="U-Turn Right" beats="1.5" />
      <move select="Forward 2" />
      <move select="Flip Right" beats="4" scaleY="2" />
    </path>
    <path>
      <move select="Flip Right" beats="4" scaleY="2" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="U-Turn Right" beats="1.5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY="2" />
      <move select="Flip Right" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Circulate (Right-Hand O)" group="&quot;O&quot;"
       from="Right-Hand O"
       fractions="1.5" >
    <formation>
      <dancer gender="girl" x="-3" y="1" angle="0"/>
      <dancer gender="boy" x="-3" y="-1" angle="180"/>
      <dancer gender="boy" x="-1" y="3" angle="0"/>
      <dancer gender="girl" x="-1" y="-3" angle="180"/>
    </formation>
    <path couples="2 4">
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2" />
    </path>
    <path couples="3 1">
      <move select="Flip Right" />
    </path>
    <path couples="2 4">
      <move select="Forward 2" beats="3" />
    </path>
    <path couples="3 1">
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Circulate (Left-Hand O)" group="&quot;O&quot;"
       from="Left-Hand O"
       formation="O LH"
       fractions="1.5" >
    <path>
      <move select="Flip Left" />
    </path>
    <path>
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Forward 2" beats="3" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Cloverleaf" group="&quot;O&quot;"
       formation="O Out">
    <path>
      <move select="Lead Right" />
      <move select="Lead Right" scaleX="1.5" scaleY="1.5" />
      <move select="Lead Right" scaleX="1.5" scaleY="1.5" />
      <move select="Extend Right" beats="2" scaleY="2" />
    </path>
    <path>
      <move select="Lead Left" />
      <move select="Lead Left" scaleX="1.5" scaleY="1.5" />
      <move select="Lead Left" scaleX="1.5" scaleY="1.5" />
      <move select="Extend Left" beats="2" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2" />
      <move select="Lead Right" />
      <move select="Lead Right" scaleX="1.5" scaleY="1.5" />
      <move select="Lead Right" scaleX="1.5" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Right" beats="2" scaleX="2" scaleY="2" />
      <move select="Lead Left" />
      <move select="Lead Left" scaleX="1.5" scaleY="1.5" />
      <move select="Lead Left" scaleX="1.5" scaleY=".5" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Coordinate" group="&quot;O&quot;"
       formation="O RH">
    <path>
      <move select="Forward 3" beats="4" />
      <move select="Swing Right" scaleX=".5" />
      <move select="Extend Left" beats="2" scaleY="2" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Forward" />
      <move select="Swing Right" scaleX=".5" offsetX="-1" />
    </path>
    <path>
      <move select="Extend Right" beats="4" scaleX="3" scaleY="2" />
      <move select="Swing Right" scaleX=".5" offsetX="1" />
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX="3" scaleY="3" />
      <move select="Lead Right" beats="4" scaleX="3" scaleY="3" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Double Pass Thru" group="&quot;O&quot;"
       formation="O In">
    <path numbers="6 2">
      <move select="Extend Left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path numbers="5 1">
      <move select="Forward 2" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY="2" />
    </path>
    <path numbers="7 3">
      <move select="Extend Left" scaleY=".5" />
      <move select="Extend Right" beats="3" scaleX="3" scaleY="2.5" />
    </path>
    <path numbers="4 8">
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2" />
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Pass In" group="&quot;O&quot;"
       formation="O Eight Chain Thru">
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Quarter Right" offsetX="1" />
    </path>
    <path>
      <move select="Quarter Left" beats="3.5" offsetX="2" offsetY="-2" />
    </path>
    <path>
      <move select="Lead Right" beats="2" scaleX="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Quarter Left" beats="3.5" offsetX="2" offsetY="2" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Pass Thru" group="&quot;O&quot;"
       formation="O Eight Chain Thru">
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Forward" />
      <move select="Extend Right" beats="2" scaleY="2" />
    </path>
    <path>
      <move select="Forward" />
      <move select="Extend Right" beats="2" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Forward" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Peel Off" group="&quot;O&quot;"
       formation="O RH">
    <path>
      <move select="U-Turn Left" beats="4" />
    </path>
    <path>
      <move select="Run Left" beats="4" offsetX="-2" />
    </path>
    <path>
      <move select="U-Turn Left" beats="4" />
    </path>
    <path>
      <move select="U-Turn Left" beats="4" offsetX="2" offsetY="-2" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Right Roll to a Wave" group="&quot;O&quot;"
       formation="O Trade By">
    <path>
      <move select="U-Turn Right" />
    </path>
    <path>
      <move select="Run Right" offsetX="-2" />
    </path>
    <path>
      <move select="Run Right" offsetX="-2" />
    </path>
    <path>
      <move select="U-Turn Right" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Scoot Back" group="&quot;O&quot;"
       formation="O RH">
    <path>
      <move select="Extend Right" beats="2" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Run Right" />
    </path>
    <path>
      <move select="Run Right" beats="6" scaleX=".5" scaleY="3" />
    </path>
    <path>
      <move select="Extend Right" beats="2" scaleY="2" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Extend Left" beats="3" scaleY="3" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Slide Thru" group="&quot;O&quot;"
       formation="O Eight Chain Thru">
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Quarter Right" offsetX="1" />
    </path>
    <path>
      <move select="Quarter Left" beats="3.5" offsetX="2" offsetY="-2" />
    </path>
    <path>
      <move select="Lead Right" beats="2.5" scaleX="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Quarter Left" beats="3.5" offsetX="2" offsetY="2" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Square Thru" group="&quot;O&quot;"
       formation="O Eight Chain Thru">
    <path>
      <move select="Extend Left" beats="2" />
      <move select="Lead Right" beats="2" scaleX=".5" scaleY="2" />
      <move select="Lead Right" beats="2" scaleX="1.5" scaleY=".5" />
      <move select="Lead Right" beats="2" scaleX=".5" scaleY="1.5" />
      <move select="Extend Left" beats="2" scaleY=".5" />
    </path>
    <path>
      <move select="Forward" beats="2" />
      <move select="Lead Left" beats="2" scaleX="1.5" />
      <move select="Lead Left" beats="2" scaleX=".5" scaleY="1.5" />
      <move select="Lead Left" beats="2" scaleX="1.5" scaleY=".5" />
      <move select="Extend Left" beats="2" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Right" beats="2" />
      <move select="Lead Right" beats="2" scaleX=".5" scaleY="2" />
      <move select="Lead Right" beats="2" scaleX="1.5" scaleY=".5" />
      <move select="Lead Right" beats="2" scaleX=".5" scaleY="1.5" />
      <move select="Extend Left" beats="2" scaleX="3" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Lead Left" beats="2" scaleX="1.5" />
      <move select="Lead Left" beats="2" scaleX=".5" scaleY="1.5" />
      <move select="Lead Left" beats="2" scaleX="1.5" scaleY=".5" />
      <move select="Extend Left" beats="2" scaleX="3" scaleY=".5" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Split Circulate" group="&quot;O&quot;"
       formation="O RH">
    <path>
      <move select="Extend Left" beats="6" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Run Right" beats="6"/>
    </path>
    <path>
      <move select="Flip Right" beats="6" scaleY="3" />
    </path>
    <path>
      <move select="Extend Right" beats="6" scaleX="2" scaleY="2" />
    </path>
  </tam>


  <tam title="&quot;O&quot; Swap Around" group="&quot;O&quot;"
       formation="O Eight Chain Thru">
    <path>
      <move select="Flip Right" />
    </path>
    <path>
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Flip Right" beats="5" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Swing Thru" group="&quot;O&quot;"
       formation="O Eight Chain Thru">
    <path numbers="6 2">
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Swing Right" />
      <move select="Swing Left" offsetX="1" />
    </path>
    <path numbers="5 1">
      <move select="Forward" beats="2" />
      <move select="Swing Right" />
      <move select="Back" beats="3" />
    </path>
    <path numbers="4 8">
      <move select="Forward" beats="2" />
      <move select="Swing Right" />
      <move select="Swing Left" offsetX="-1" />
    </path>
    <path numbers="7 3">
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Swing Right" />
      <move select="Forward" beats="3" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Touch a Quarter" group="&quot;O&quot;"
       formation="O Eight Chain Thru">
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Quarter Right" offsetX="1" />
    </path>
    <path>
      <move select="Lead Right" beats="2" scaleX="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Lead Right" beats="2" scaleX="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Quarter Right" offsetX="1" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Track Two" group="&quot;O&quot;"
       formation="O Out">
    <path>
      <move select="Flip Left" offsetX="-2" />
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Run Right" beats="5" scaleY="2" />
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Run Left" beats="6" offsetX="2" offsetY="2" />
    </path>
    <path>
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Trade By" group="&quot;O&quot;"
       formation="O Trade By">
    <path>
      <move select="Run Right" />
    </path>
    <path>
      <move select="Flip Left" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Extend Right" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Extend Right" scaleY=".5" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Transfer the Column" group="&quot;O&quot;">
    <formation>
      <dancer gender="girl" x="-3" y="1" angle="0"/>
      <dancer gender="boy" x="-3" y="-1" angle="180"/>
      <dancer gender="boy" x="-1" y="3" angle="0"/>
      <dancer gender="girl" x="-1" y="-3" angle="180"/>
    </formation>
    <path couples="2 4">
      <move select="Forward 2" />
      <move select="Swing Right" scaleX=".5" />
      <move select="Hinge Right" scaleX=".5" />
      <move select="Extend Left" beats="2" scaleY="1.5" />
    </path>
    <path couples="3 1">
      <move select="Run Right" beats="3" scaleY="2" />
      <move select="Forward 4" />
      <move select="Lead Right" beats="2" scaleX="2" scaleY="2" />
    </path>
    <path couples="2 4">
      <move select="Extend Right" beats="2" scaleX="2" scaleY="2" />
      <move select="Swing Right" scaleX=".5" />
      <move select="Hinge Right" scaleX=".5" />
      <move select="Extend Right" beats="4" scaleX="3" scaleY=".5" />
    </path>
    <path couples="3 1">
      <move select="Forward 2" />
      <move select="Run Right" beats="4" scaleY="3.5" />
      <move select="Lead Right" beats="4" scaleX="2" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Veer Left" group="&quot;O&quot;"
       from=""
       formation="O Eight Chain Thru"
       fractions="" >
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Dodge Left" />
    </path>
    <path>
    </path>
    <path>
      <move select="Extend Left" beats="4" scaleX="2" scaleY="4" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Walk and Dodge" group="&quot;O&quot;"
       formation="O RH" >
    <path numbers="1 8">
      <move select="Extend Left" beats="4" scaleX="2" scaleY="2" />
    </path>
    <path numbers="2 7">
      <move select="Dodge Right" beats="4" />
    </path>
    <path numbers="3 6">
      <move select="Dodge Right" beats="4" scaleY="3" />
    </path>
    <path numbers="4 5">
      <move select="Extend Right" beats="4" scaleX="2" scaleY="2" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Wheel Thru" group="&quot;O&quot;"
       formation="O Eight Chain Thru">
    <path>
      <move select="Lead Right" beats="3" scaleX="2" scaleY="2" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Quarter Right" />
    </path>
    <path>
      <move select="Lead Right" beats="3.5" scaleY="3" />
      <move select="Extend Left" beats="1.5" />
    </path>
    <path>
      <move select="Quarter Right" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Zoom" group="&quot;O&quot;"
       formation="O In">
    <path>
      <move select="Extend Left" beats="6" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Extend Right" beats="6" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Run Left" offsetX="-1" offsetY="-1" />
      <move select="Run Left" offsetX="1" offsetY="1" />
    </path>
    <path>
      <move select="Run Right" offsetX="-1" offsetY="1" />
      <move select="Run Right" offsetX="1" offsetY="-1" />
    </path>
  </tam>

  <tamxref title="&quot;O&quot; (C-1) Cross and Turn" group="&quot;O&quot; (C-1)"
           xref-link="c1/cross_and_turn"
           xref-title="&quot;O&quot; Cross and Turn"/>

  <tamxref title="&quot;O&quot; (C-1) Reverse Cross and Turn" group="&quot;O&quot; (C-1)"
           xref-link="c1/reverse_cross_and_turn"
           xref-title="&quot;O&quot; Reverse Cross and Turn"/>

  <tamxref title="&quot;O&quot; (C-1) Shakedown" group="&quot;O&quot; (C-1)"
           xref-link="c1/shakedown"
           xref-title="&quot;O&quot; Shakedown"/>

  <tamxref title="&quot;O&quot; (C-1) Wheel Fan Thru" group="&quot;O&quot; (C-1)"
           xref-link="c1/wheel_fan_thru"
           xref-title="&quot;O&quot; Wheel Fan Thru"/>

  <tamxref title="&quot;O&quot; (C-1) Left Wheel Fan Thru" group="&quot;O&quot; (C-1)"
           xref-link="c1/wheel_fan_thru"
           xref-title="&quot;O&quot; Left Wheel Fan Thru"/>

  <tamxref title="&quot;O&quot; (C-1) Zing" group="&quot;O&quot; (C-1)"
           xref-link="c1/zing"
           xref-title="&quot;O&quot; Zing"/>

  <tam title="Magic &quot;O&quot; (C-1) Circulate" group="Magic &quot;O&quot; (C-1)">
    <formation>
      <dancer gender="boy" x="-3" y="1" angle="0"/>
      <dancer gender="girl" x="-3" y="-1" angle="180"/>
      <dancer gender="boy" x="-1" y="3" angle="180"/>
      <dancer gender="girl" x="-1" y="-3" angle="0"/>
    </formation>
    <path>
      <move select="Extend Right" beats="4" scaleX="2" scaleY="4"/>
    </path>
    <path>
      <move select="Run Right" beats="4"/>
    </path>
    <path>
      <move select="Extend Left" beats="4" scaleX="2" scaleY="4"/>
    </path>
    <path>
      <move select="Forward 2" beats="4"/>
    </path>
  </tam>

  <tam title="Magic &quot;O&quot; (C-1) Walk and Dodge" group="Magic &quot;O&quot; (C-1)">
    <formation>
      <dancer gender="boy" x="-3" y="1" angle="0"/>
      <dancer gender="girl" x="-3" y="-1" angle="180"/>
      <dancer gender="boy" x="-1" y="3" angle="180"/>
      <dancer gender="girl" x="-1" y="-3" angle="0"/>
    </formation>
    <path>
      <move select="Extend Right" beats="4" scaleX="2" scaleY="4"/>
    </path>
    <path>
      <move select="Dodge Right" beats="4"/>
    </path>
    <path>
      <move select="Extend Left" beats="4" scaleX="2" scaleY="4"/>
    </path>
    <path>
      <move select="Dodge Left" beats="4" scaleY="3"/>
    </path>
  </tam>

  <tamxref title="&quot;O&quot; (C-2) Turn to a Line" group="&quot;O&quot; (C-2)"
           xref-link="c2/turn_to_a_line"
           xref-title="&quot;O&quot; Turn to a Line"/>

</tamination>
