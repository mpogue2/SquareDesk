<tamination title="Concentric Concept">

  <tam title="(From columns) Concentric Box Counter Rotate" group="(From columns) Concentric"
       formation="Magic Column RH">
    <path>
      <move select="Counter Rotate Left 4 -2" beats="5" hands="left"/>
    </path>
    <path>
      <move select="Counter Rotate Right 0 -2" beats="5" hands="right"/>
    </path>
    <path>
      <move select="Counter Rotate Right 2 0" beats="5" hands="right"/>
    </path>
    <path>
      <move select="Counter Rotate Left -2 4" beats="5" hands="left"/>
    </path>
    <taminator>
    Same as Counter Rotate
    </taminator>
  </tam>

  <tam title="(From columns) Concentric Box Transfer" group="(From columns) Concentric"
       formation="Column RH GBGB">
    <path>
      <move select="Run Right" beats="6" scaleY="2.5"/>
      <move select="Forward 3" beats="5"/>
