<tamination title="Butterfly Formation / Concept">

  <tam title="Butterfly Chase Right" group="Butterfly" from=""
       formation="Butterfly Trade By">
    <path>
      <move select="Run Right" beats="4" scaleY="2"/>
      <move select="Forward 2"/>
    </path>
    <path>
      <move select="U-Turn Right" offsetX="-2" offsetY="1"/>
      <move select="Flip Right" scaleY="1.5"/>
    </path>
    <path>
      <move select="Run Right" beats="6" scaleY="2" offsetX="-2"/>
    </path>
    <path>
      <move select="U-Turn Right" offsetX="-2"/>
      <move select="Run Right" scaleY="2"/>
    </path>
  </tam>

  <tam title="Butterfly Checkmate" group="Butterfly">
    <formation>
      <dancer gender="boy" y="3" x="3" angle="270"/>
      <dancer gender="girl" y="3" x="-3" angle="90"/>
      <dancer gender="girl" y="1" x="1" angle="270"/>
      <dancer gender="boy" y="1" x="-1" angle="90"/>
    </formation>
    <path couples="1 3">
      <move select="Extend Right" scaleX="2" scaleY="2" beats="7"/>
      <move select="Forward"/>
      <move select="Lead Right" beats="2"/>
      <move select="Forward"/>
    </path>
    <path couples="4 2">
      <move select="Run Right" beats="6" scaleY="2.5"/>
      <move select="Forward 4"/>
      <move select="Quarter Right" beats="3" offsetX="2" offsetY="1"/>
    </path>
    <path couples="1 3">
      <move select="Forward 2" beats="7"/>
      <move select="Forward"/>
      <move select="Lead Right" beats="2"/>
      <move select="Forward 3"/>
    </path>
    <path couples="4 2">
      <move select="Stand" beats="2"/>
      <move select="Run Right" beats="7.5" scaleX="2" scaleY="1.5" offsetX=".5"/>
      <move select="Lead Right" beats="2.5" scaleX="2.5"/>
    </path>
  </tam>

  <tam title="Butterfly Circulate" group="Butterfly">
    <formation>
      <dancer gender="girl" x="-3" y="3" angle="0"/>
      <dancer gender="boy" x="-3" y="-3" angle="180"/>
      <dancer gender="boy" x="-1" y="1" angle="0"/>
      <dancer gender="girl" x="-1" y="-1" angle="180"/>
    </formation>
    <path couples="2 4">
      <move select="Extend Right" beats="2" scaleX="2" scaleY="2" />
    </path>
    <path couples="3 1">
      <move select="Run Right" beats="3" scaleY="3" />
    </path>
    <path couples="2 4">
      <move select="Forward 2" />
    </path>
    <path couples="3 1">
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2" />
    </path>
  </tam>

  <tam title="Butterfly Cloverleaf" group="Butterfly" from="" formation="Butterfly Out">
    <path>
      <move select="Run Left"/>
      <move select="Lead Left" beats="3" scaleX="2" scaleY="2"/>
      <move select="Forward 2"/>
    </path>
    <path>
      <move select="Run Right"/>
      <move select="Lead Right" beats="3" scaleX="2" scaleY="2"/>
      <move select="Forward 2"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Lead Left" beats="3" scaleX="2" scaleY="2"/>
      <move select="Run Left"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Lead Right" beats="3" scaleX="2" scaleY="2"/>
      <move select="Run Right"/>
    </path>
  </tam>

  <tam title="Butterfly Coordinate" group="Butterfly" from="" formation="Butterfly RH">
    <path>
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2"/>
      <move select="Forward"/>
      <move select="Swing Right" scaleX=".5"/>
      <move select="Extend Left" beats="3" scaleX="3" scaleY="2"/>
    </path>
    <path>
      <move select="Run Right" scaleY="2"/>
      <move select="Forward"/>
      <move select="Swing Right" scaleX=".5"/>
      <move select="Back" hands="right" beats="3"/>
    </path>
    <path>
      <move select="Forward 3" beats="4"/>
      <move select="Swing Right" scaleX=".5"/>
      <move select="Forward" hands="right" beats="3"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Lead Right" beats="2"/>
      <move select="Lead Right" beats="5" scaleX="3"/>
    </path>
  </tam>

  <tam title="Butterfly Double Pass Thru" group="Butterfly" from="" formation="Butterfly In">
    <path numbers="1 8">
      <move select="Forward 3"/>
      <move select="Extend Right" beats="2" scaleY="2"/>
    </path>
    <path numbers="2 7">
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2"/>
      <move select="Forward 2"/>
    </path>
    <path numbers="3 6">
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2"/>
      <move select="Forward 2"/>
    </path>
    <path numbers="4 5">
      <move select="Extend Left" scaleY=".5"/>
      <move select="Extend Right" beats="4" scaleX="3" scaleY="2.5"/>
    </path>
  </tam>

  <tam title="Butterfly Horseshoe Turn" group="Butterfly" formation="Butterfly Out" from="">
    <path>
      <move select="Quarter Left" />
      <move select="Quarter Left" />
      <move select="Quarter Left" />
    </path>
    <path>
      <move select="Quarter Right" />
      <move select="Quarter Right" />
      <move select="Quarter Right" />
    </path>
    <path>
      <move select="Quarter Right" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Extend Right" scaleY=".5" />
    </path>
    <path>
      <move select="Quarter Left" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Extend Right" scaleY=".5" />
    </path>
  </tam>

  <tam title="Butterfly Pass In" group="Butterfly" from="" formation="Butterfly Chain Thru">
    <path>
      <move select="Lead Right" beats="3" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Lead Left" scaleX=".5" beats="1"/>
      <move select="Extend Right" beats="2" scaleY="1.5"/>
    </path>
    <path>
      <move select="Lead Left" beats="1" scaleX=".5"/>
      <move select="Run Right" beats="2" offsetX="1" scaleY=".75"/>
    </path>
    <path>
      <move select="Lead Right" beats="1"/>
      <move select="Run Left" beats="2" offsetX="1" scaleY=".5"/>
    </path>
  </tam>

  <tam title="Butterfly Pass Thru" group="Butterfly" from="" formation="Butterfly Chain Thru">
    <path>
      <move select="Forward"/>
      <move select="Extend Right" beats="2" scaleY="2"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="2"/>
      <move select="Forward"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="2"/>
      <move select="Forward"/>
    </path>
    <path>
      <move select="Forward"/>
      <move select="Extend Right" beats="2" scaleY="2"/>
    </path>
  </tam>

  <tam title="Butterfly Peel Off" group="Butterfly" from="" formation="Butterfly RH">
    <path>
      <move select="U-Turn Left" offsetX="2" offsetY="-2"/>
    </path>
    <path>
      <move select="U-Turn Left"/>
    </path>
    <path>
      <move select="Run Left" offsetX="-2"/>
    </path>
    <path>
      <move select="U-Turn Left"/>
    </path>
  </tam>

  <tam title="Butterfly Remake" group="Butterfly" from="" formation="Butterfly RH">
    <path>
      <move select="Lead Right" beats="3" scaleX=".5" scaleY="3"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Hinge Right" scaleY=".5"/>
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX=".5" scaleY="3"/>
      <move select="Stand" beats="3"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Lead Right" beats="3" scaleX="3" scaleY=".5"/>
    </path>
    <path>
      <move select="Hinge Right" beats="3" scaleX=".5"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Hinge Right" scaleY=".5"/>
    </path>
    <path>
      <move select="Hinge Right" beats="3" scaleX=".5"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Lead Right" beats="3" scaleX="3" scaleY=".5"/>
    </path>
  </tam>

  <tam title="Butterfly Right Roll to a Wave" group="Butterfly" from="" formation="Butterfly Trade By">
    <path>
      <move select="Run Right" offsetX="-2"/>
    </path>
    <path>
      <move select="U-Turn Right"/>
    </path>
    <path>
      <move select="U-Turn Right"/>
    </path>
    <path>
      <move select="Run Right" offsetX="-2"/>
    </path>
  </tam>

  <tam title="Butterfly Scoot Back" group="Butterfly" from="" formation="Butterfly RH">
    <path>
      <move select="Extend Right" beats="2" scaleX=".5" scaleY="2.5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Extend Left" beats="2" scaleX=".5" scaleY="2.5"/>
    </path>
    <path>
      <move select="Run Right" beats="6" scaleX="2" scaleY="3"/>
    </path>
    <path>
      <move select="Run Right" beats="6" scaleX=".5"/>
    </path>
    <path>
      <move select="Extend Right" beats="2" scaleX="1.5" scaleY=".5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Extend Left" beats="2" scaleX="1.5" scaleY=".5"/>
    </path>
  </tam>

  <tam title="Butterfly Slide Thru" group="Butterfly" from="" formation="Butterfly Chain Thru">
    <path>
      <move select="Lead Right" beats="2.5" scaleX="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Quarter Left" beats="3.5" offsetX="2" offsetY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Quarter Right" offsetX="1" />
    </path>
    <path>
      <move select="Quarter Left" beats="3.5" offsetX="2" offsetY="-2" />
    </path>
  </tam>

  <tam title="Butterfly Split Circulate" group="Butterfly"
       from="Butterfly"
       formation="Butterfly RH"
       fractions="" >
    <path>
      <move select="Extend Right" beats="2" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Run Right" beats="3" scaleY="3" />
    </path>
    <path>
      <move select="Flip Right" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2" />
    </path>
  </tam>

  <tam title="Butterfly Square Thru" group="Butterfly" from="" formation="Butterfly Chain Thru">
    <path>
      <move select="Extend Right" beats="2" />
      <move select="Lead Right" beats="2" scaleX=".5" scaleY="2" />
      <move select="Lead Right" beats="2" scaleX="1.5" scaleY=".5" />
      <move select="Lead Right" beats="2" scaleX=".5" scaleY="1.5" />
      <move select="Extend Left" beats="2" scaleX="3" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Lead Left" beats="2" scaleX="1.5" />
      <move select="Lead Left" beats="2" scaleX=".5" scaleY="1.5" />
      <move select="Lead Left" beats="2" scaleX="1.5" scaleY=".5" />
      <move select="Extend Left" beats="2" scaleX="3" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" />
      <move select="Lead Right" beats="2" scaleX=".5" scaleY="2" />
      <move select="Lead Right" beats="2" scaleX="1.5" scaleY=".5" />
      <move select="Lead Right" beats="2" scaleX=".5" scaleY="1.5" />
      <move select="Extend Left" beats="2" scaleY=".5" />
    </path>
    <path>
      <move select="Forward" beats="2" />
      <move select="Lead Left" beats="2" scaleX="1.5" />
      <move select="Lead Left" beats="2" scaleX=".5" scaleY="1.5" />
      <move select="Lead Left" beats="2" scaleX="1.5" scaleY=".5" />
      <move select="Extend Left" beats="2" scaleY=".5" />
    </path>
  </tam>

  <tam title="Butterfly Swap Around" group="Butterfly" from="" formation="Butterfly Chain Thru">
    <path>
      <move select="Flip Right" beats="5" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Flip Right" />
    </path>
    <path>
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2" />
    </path>
  </tam>

  <tam title="Butterfly Swing Thru" group="Butterfly" from="" formation="Butterfly Chain Thru">
    <path>
      <move select="Forward" beats="2" />
      <move select="Swing Right" />
      <move select="Swing Left" offsetX="-1" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Swing Right" />
      <move select="Forward" beats="3" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Swing Right" />
      <move select="Swing Left" offsetX="1" />
    </path>
    <path>
      <move select="Forward" beats="2" />
      <move select="Swing Right" />
      <move select="Back" beats="3" />
    </path>
  </tam>

  <tam title="Butterfly Touch a Quarter" group="Butterfly" from="" formation="Butterfly Chain Thru">
    <path>
      <move select="Lead Right" beats="2" scaleX="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Quarter Right" offsetX="1" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Quarter Right" offsetX="1" />
    </path>
    <path>
      <move select="Lead Right" beats="2" scaleX="2" />
      <move select="Forward" />
    </path>
  </tam>

  <tam title="Butterfly Track Two" group="Butterfly" from="" formation="Butterfly Out">
    <path>
      <move select="Run Right" beats="6" scaleY="3"/>
      <move select="Forward 6"/>
    </path>
    <path>
      <move select="Flip Left" beats="4" scaleY="2" offsetX="-1"/>
      <move select="Forward 3"/>
    </path>
    <path>
      <move select="Extend Left" beats="6" scaleX="2" scaleY="2"/>
      <move select="Run Right" beats="6" scaleY="3"/>
    </path>
    <path>
      <move select="Stand" beats="4"/>
      <move select="Run Left" beats="4"/>
    </path>
  </tam>

  <tam title="Butterfly Trade By" group="Butterfly" from="" formation="Butterfly Trade By">
    <path>
      <move select="Run Right" beats="6" scaleY="3"/>
    </path>
    <path>
      <move select="Flip Left" beats="6" scaleY="3"/>
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" beats="3"/>
      <move select="Extend Right" scaleY=".5" beats="3"/>
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" beats="3"/>
      <move select="Extend Right" scaleY=".5" beats="3"/>
    </path>
  </tam>

  <tam title="Butterfly Transfer the Column" group="Butterfly" formation="Butterfly RH" from="">
    <path numbers="1 8">
      <move select="Extend Right" beats="4" scaleX="2" scaleY="2"/>
      <move select="Cast Right" beats="6" hands="gripright" scaleX=".5"/>
      <move select="Extend Left" beats="4" scaleX="3" scaleY="1.5"/>
    </path>
    <path numbers="2 7">
      <move select="Run Right" beats="6" scaleY="3"/>
      <move select="Forward 5"/>
      <move select="Quarter Right" offsetX="1"/>
    </path>
    <path numbers="3 6">
        <move select="Forward 2" beats="4"/>
      <move select="Cast Right" beats="6" hands="gripright" scaleX=".5"/>
      <move select="Extend Right" scaleY=".5"/>
    </path>
    <path numbers="4 5">
      <move select="Forward 2" beats="4"/>
      <move select="Run Right" beats="6" scaleY="2"/>
      <move select="Lead Right" beats="4" scaleX="2" scaleY="2"/>
    </path>
  </tam>

  <tam title="Butterfly Veer Left" group="Butterfly" from="" formation="Butterfly Chain Thru">
    <path>
    </path>
    <path>
      <move select="Extend Left" beats="4" scaleX="2" scaleY="4" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Dodge Left" />
    </path>
  </tam>

  <tam title="Butterfly Walk and Dodge" group="Butterfly" from="" formation="Butterfly RH">
    <path>
      <move select="Extend Right" beats="4" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Dodge Right" beats="4" scaleY="3" />
    </path>
    <path>
      <move select="Dodge Right" beats="4" />
    </path>
    <path>
      <move select="Extend Left" beats="4" scaleX="2" scaleY="2" />
    </path>
  </tam>

  <tam title="Butterfly Wheel Thru" group="Butterfly" from="" formation="Butterfly Chain Thru">
    <path>
      <move select="Lead Right" beats="3.5" scaleY="3" />
      <move select="Extend Left" beats="1.5" />
    </path>
    <path>
      <move select="Quarter Right" />
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX="2" scaleY="2" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Quarter Right" />
    </path>
  </tam>

  <tamxref title="Butterfly (C-1) Cross and Turn" group="Butterfly (C-1)"
           xref-link="c1/cross_and_turn"
           xref-title="Butterfly Cross and Turn"/>

  <tamxref title="Butterfly (C-1) Reverse Cross and Turn" group="Butterfly (C-1)"
           xref-link="c1/reverse_cross_and_turn"
           xref-title="Butterfly Reverse Cross and Turn"/>

  <tamxref title="Butterfly (C-1) Shakedown" group="Butterfly (C-1)"
           xref-link="c1/shakedown"
           xref-title="Butterfly Shakedown"/>

  <tamxref title="Butterfly (C-1) Wheel Fan Thru" group="Butterfly (C-1)"
           xref-link="c1/wheel_fan_thru"
           xref-title="Butterfly Wheel Fan Thru"/>

  <tamxref title="Butterfly (C-1) Left Wheel Fan Thru" group="Butterfly (C-1)"
           xref-link="c1/wheel_fan_thru"
           xref-title="Butterfly Left Wheel Fan Thru"/>

  <tamxref title="Butterfly (C-1) Zing" group="Butterfly (C-1)"
           xref-link="c1/zing"
           xref-title="Butterfly Zing"/>

  <tamxref title="Butterfly (C-2) Turn to a Line" group="Butterfly (C-2)"
           xref-link="c2/turn_to_a_line"
           xref-title="Butterfly Turn to a Line"/>

</tamination>
