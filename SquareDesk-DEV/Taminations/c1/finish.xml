<tamination title="Finish">

  <tam title="Finish Alter the Wave" group="Finish"
       formation="Ocean Waves LH BGGB"
       parts="4.5;4" >
    <path>
      <move select="U-Turn Left" beats="4.5" />
      <move select="Lead Right" beats="2" scaleX="1.5" scaleY="3" />
      <move select="Lead Right" beats="2" scaleX="3" scaleY="1.5" />
      <move select="Flip Right" />
    </path>
    <path>
      <move select="Cast Right" />
      <move select="Swing Right" beats="4" />
      <move select="Lead Right" beats="3" scaleX="3" />
    </path>
    <path>
      <move select="Cast Right" />
      <move select="Swing Right" beats="4" />
      <move select="Lead Right" beats="3" scaleX="3" />
    </path>
    <path>
      <move select="U-Turn Left" beats="4.5" />
      <move select="Lead Right" beats="2" scaleX="2" scaleY="3" />
      <move select="Lead Right" beats="2" scaleX="3" scaleY="2" />
      <move select="Flip Right" />
    </path>
  </tam>

  <tam title="Finish Brace Thru" group="Finish">
     <formation>
      <dancer gender="boy" x="2" y="3" angle="0"/>
      <dancer gender="girl" x="2" y="1" angle="0"/>
      <dancer gender="boy" x="2" y="-3" angle="0"/>
      <dancer gender="girl" x="2" y="-1" angle="0"/>
    </formation>
    <path>
      <move select="Beau Wheel"/>
    </path>
    <path>
      <move select="Belle Wheel"/>
    </path>
    <path>
      <move select="U-Turn Left"/>
    </path>
    <path>
      <move select="U-Turn Right"/>
    </path>
  </tam>

  <tam title="Finish Coordinate" group="Finish"
       formation="Column RH GBGB"
       parts="1;3">
    <path>
      <move select="Lead Right" scaleX=".5" />
      <move select="Lead Right" scaleX="3" scaleY="1.5" beats="5" />
    </path>
    <path>
      <move select="Forward" />
      <move select="Hinge Right" scaleX=".5" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
    <path>
      <move select="Forward" />
      <move select="Hinge Right" scaleX=".5" />
      <move select="Lead Right" scaleX="3" scaleY="2.5" beats="5" />
    </path>
    <path>
      <move select="Forward" />
      <move select="Hinge Right" scaleX=".5" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
    <taminator>
    The first part of Coordinate is Circulate, and the second part is
    Circulate 1/2.  So Finish Coordinate starts with Circulate 1/2.
    </taminator>
  </tam>

  <tam title="Finish Cross Chain And Roll" group="Finish"
       formation="Trade By">
    <path>
      <move select="Belle Wheel" />
      <move select="Quarter Left" />
    </path>
    <path>
      <move select="Beau Wheel" />
      <move select="Quarter Left" />
    </path>
    <path>
      <move select="Forward" beats="1.5"/>
      <move select="Hinge Left" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Quarter Right" offsetY="-1" />
      <move select="Hinge Left" />
      <move select="Quarter Left" offsetX="1" />
    </path>
  </tam>

  <tam title="Finish Diamond Chain Thru" group="Finish"
       formation="Diamonds RH Girl Points"
       parts="3" >
    <path>
      <move select="Stand" beats="3" />
      <move select="Cast Right" />
    </path>
    <path>
    </path>
    <path>
      <move select="Swing Left" />
      <move select="Cast Right" />
    </path>
    <path>
    </path>
  </tam>

  <tam title="Finish Dixie Diamond" group="Finish"
       formation="Ocean Waves LH GBBG">
    <path>
      <move select="U-Turn Left" />
    </path>
    <path>
      <move select="Hinge Right" />
    </path>
    <path>
      <move select="Hinge Right" />
    </path>
    <path>
      <move select="U-Turn Left" />
    </path>
  </tam>

  <tam title="Finish Linear Cycle"
       group="Finish"
       formation="Ocean Waves RH BGGB"
       fractions="4" >
    <path>
      <move select="Forward 4" />
      <move select="Run Right" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Forward 2" />
      <move select="U-Turn Right" offsetX="2" />
    </path>
    <path>
      <move select="Forward 4" />
      <move select="Run Right" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Forward 2" />
      <move select="U-Turn Right" offsetX="2" />
    </path>
  </tam>

  <tam title="Finish Load the Boat" group="Finish"
       formation="Lines Facing Out" fractions="2;3">
    <path>
      <move select="Lead Left" scaleX="1.5" scaleY="3.5" beats="2" />
      <move select="Lead Left" scaleX="2.5" scaleY="3.5" beats="2.5" />
      <move select="Quarter Left" beats="2.5" offsetX="1" />
    </path>
    <path>
      <move select="Quarter Right" beats="2" offsetX="-1" />
      <move select="Run Right" />
      <move select="Pass Thru" />
    </path>
    <path>
      <move select="Quarter Left" beats="2" offsetX="-1"/>
      <move select="Flip Left" />
      <move select="Pass Thru" />
    </path>
    <path>
      <move select="Lead Right" scaleX="2.5" scaleY="3.5" beats="2" />
      <move select="Lead Right" scaleX="3.5" scaleY="4.5" beats="2.5" />
      <move select="Lead Right" beats="2.5" />
    </path>
  </tam>

  <tam title="Finish Motivate" group="Finish"
       formation="Ocean Waves RH BGGB"
       fractions="4.5;3" >
    <path numbers="4 5" couples="3 1">
      <move select="Extend Right" beats="4.5" scaleX="2" scaleY="2" />
      <move select="Swing Right" />
      <move select="Extend Left" beats="4.5" scaleX="2" scaleY="2" />
    </path>
    <path numbers="3 6" couples="3 1">
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Cast Left" />
    </path>
    <path numbers="2 7" couples="2 4">
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
    </path>
    <path numbers="1 8" couples="2 4">
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
      <move select="Swing Right" />
      <move select="Cast Left" />
    </path>
  </tam>

  <tam title="Finish Pass and Roll" group="Finish"
       formation="Trade By" >
    <path>
      <move select="Flip Right" scaleY=".25" />
      <move select="Forward 3" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY="1.5" />
    </path>
    <path>
      <move select="Flip Right" scaleY=".25" />
      <move select="Forward 3" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Forward" />
      <move select="Extend Right" scaleY=".5" />
      <move select="U-Turn Right" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Forward" />
      <move select="Extend Right" scaleY=".5" />
      <move select="Flip Right" />
    </path>
  </tam>

  <tam title="Finish Pass the Ocean" group="Finish"
       formation="Lines Facing Out">
    <path>
      <move select="Quarter Left" beats="2" offsetX="-1" offsetY="1" />
    </path>
    <path>
      <move select="Lead Right" beats="2" />
    </path>
    <path>
      <move select="Quarter Left" beats="2" offsetX="-1" offsetY="1" />
    </path>
    <path>
      <move select="Lead Right" beats="2" />
    </path>
  </tam>

  <tam title="Finish Pass the Sea" group="Finish"
       formation="Lines Facing Out">
    <path>
      <move select="Lead Left" beats="2" />
    </path>
    <path>
      <move select="Quarter Right" beats="2" offsetX="-1" offsetY="-1" />
    </path>
    <path>
      <move select="Lead Left" beats="2" />
    </path>
    <path>
      <move select="Quarter Right" beats="2" offsetX="-1" offsetY="-1" />
    </path>
  </tam>

  <tam title="Finish Percolate" group="Finish"
       formation="Ocean Waves RH BGGB"
       parts="2" >
    <path>
      <move select="Forward 2" />
      <move select="Hinge Right" />
      <move select="Extend Right" hands="left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Lead Left" beats="2" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" />
    </path>
    <path>
      <move select="Forward 2" />
      <move select="Hinge Right" />
      <move select="Stand" hands="none" beats="4"/>
    </path>
    <path>
      <move select="Lead Right" beats="2" scaleX="2" scaleY="3" />
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="Finish Recycle" group="Finish"
       formation="Box RH" parts="2">
    <path>
      <move select="Counter Rotate Right 3 1" offsetY="-1"/>
      <move select="Quarter Right" beats="2"/>
    </path>
    <path>
      <move select="Counter Rotate Right -1 -3" offsetY="1"/>
      <move select="Quarter Right"  beats="2"/>
    </path>
  </tam>

  <tam title="Finish Relay the Deucey" group="Finish"
    formation="Ocean Waves RH BGGB" parts="4.5;3;3;3">
    <path>
      <move select="Forward 2" beats="4.5" />
      <move select="Forward 2" beats="3" />
      <move select="Lead Right" beats="3" scaleX="3" scaleY="3" />
      <move select="Swing Right" />
      <move select="Cast Left" />
    </path>
    <path>
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Swing Left" />
      <move select="Swing Right" />
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Lead Right" beats="3" scaleX="3" scaleY="3" />
      <move select="Forward 2" beats="3" />
      <move select="Forward 2" beats="4.5" />
    </path>
    <path>
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
      <move select="Swing Right" />
      <move select="Swing Left" />
      <move select="Swing Right" />
      <move select="Cast Left" />
    </path>
  </tam>

  <tam title="Finish Relay the Shadow" group="Finish"
       formation="Tidal Wave RH BGGB"
       parts="4.5" >
    <path>
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3.5" />
      <move select="Hinge Right" beats="4" scaleX="3" />
    </path>
    <path>
      <move select="Swing Left" hands="gripleft" scaleX=".5" scaleY=".5" />
      <move select="Hinge Left" scaleY=".5" />
      <move select="Lead Right" />
      <move select="Lead Right" />
      <move select="Lead Right" />
    </path>
    <path>
      <move select="Swing Left" hands="gripleft" scaleX=".5" scaleY=".5" />
      <move select="Hinge Left" scaleY=".5" />
      <move select="Forward 2" />
      <move select="Hinge Left" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Swing Left" hands="gripleft" scaleX=".5" scaleY=".5" />
      <move select="Hinge Left" scaleY=".5" />
      <move select="Hinge Right" beats="4" scaleX="3" />
    </path>
  </tam>

  <tam title="Finish Relay the Top" group="Finish"
       formation="Ocean Waves RH BGGB"
       parts="4.5;3" >
    <path>
      <move select="Extend Right" beats="4.5" scaleX="2" scaleY="2" />
      <move select="Hinge Right" beats="3" />
      <move select="Cast Left" />
    </path>
    <path>
      <move select="Cast Left" />
      <move select="Hinge Right" beats="3" />
      <move select="Extend Left" beats="4.5" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
      <move select="Swing Right" />
      <move select="Cast Left" />
    </path>
  </tam>

  <tam title="Finish Remake" group="Finish"
       formation="Ocean Waves LH BGGB" parts="3">
    <path>
      <move select="Swing Left"/>
      <move select="Cast Right"/>
    </path>
    <path>
      <move select="Swing Left"/>
    </path>
    <path>
      <move select="Swing Left"/>
    </path>
    <path>
      <move select="Swing Left"/>
      <move select="Cast Right"/>
    </path>
  </tam>

  <tam title="Finish Rotary Spin" group="Finish">
     <formation>
      <dancer gender="boy" x="1" y="1" angle="270"/>
      <dancer gender="girl" x="-1" y="1" angle="270"/>
      <dancer gender="boy" x="-1" y="3" angle="90"/>
      <dancer gender="girl" x="1" y="3" angle="90"/>
    </formation>
    <path numbers="5 4">
      <move select="Forward"/>
      <move select="Cast Left" />
    </path>
    <path numbers="1 8">
      <move select="Extend Right" scaleY="2"/>
      <move select="Cast Left" />
    </path>
    <path numbers="2 7">
      <move select="Beau Wheel" beats="4" />
      <move select="Quarter Left" offsetY="1" />
    </path>
    <path numbers="6 3">
      <move select="Belle Wheel" beats="4" />
      <move select="Quarter Left" offsetY="-1" />
    </path>
  </tam>

  <tam title="Finish Spin Chain and Exchange the Gears" group="Finish"
       formation="Ocean Waves RH BGGB" parts="4.5;3;4.5;4">
    <path>
      <move select="Stand" beats="2" />
      <move select="Flip Right" beats="5.5" />
      <move select="Swing Left" />
      <move select="HalfHinge Left" />
      <move select="Forward" beats="4" scaleX="2.95" />
      <move select="HalfHinge Right" />
      <move select="Hinge Right" />
      <move select="Forward" beats="2.5" scaleX="2.7" scaleY="2.7" />
      <move select="Hinge Right" hands="nohands" />
      <move select="Swing Right" hands="nohands" offsetX="-.5" />
    </path>
    <path>
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Cast Left" />
      <move select="HalfHinge Left" />
      <move select="Forward" beats="4" scaleX="2.95" />
      <move select="HalfHinge Right" />
      <move select="Hinge Right" />
      <move select="Forward" beats="2" scaleX="2.2" scaleY="2.2" />
      <move select="Lead Right" scaleX="1.5" scaleY=".5" />
    </path>
    <path>
      <move select="Cast Left" />
      <move select="Stand" beats="3" />
      <move select="Cast Left" />
      <move select="Swing Left" />
      <move select="HalfHinge Left" />
      <move select="Forward" beats="4" scaleX="2.95" />
      <move select="Forward" offsetY=".3" />
      <move select="HalfHinge Right" beats="1.5" offsetX=".3" offsetY=".3" />
    </path>
    <path>
      <move select="Stand" beats="2" />
      <move select="Flip Right" beats="5.5" />
      <move select="Swing Left" />
      <move select="Swing Left" />
      <move select="HalfHinge Left" />
      <move select="Forward" beats="4" scaleX="2.95" />
      <move select="HalfHinge Right" />
      <move select="Hinge Right" scaleX=".5" offsetY=".3" />
      <move select="Quarter Right" />
    </path>
  </tam>

  <tam title="Finish Spin Chain the Gears" group="Finish"
       formation="Ocean Waves RH BGGB" parts="4.5;3;4.5;3">
    <path>
      <move select="Stand" beats="2" />
      <move select="Flip Right" beats="5.5" />
      <move select="Cast Left" hands="gripleft"/>
      <move select="Swing Right" />
      <move select="Cast Left" />
    </path>
    <path>
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Cast Left" hands="gripleft"/>
      <move select="Flip Right" />
    </path>
    <path>
      <move select="Cast Left" />
      <move select="Stand" beats="3" />
      <move select="Cast Left" hands="gripleft"/>
      <move select="Flip Right" />
    </path>
    <path>
      <move select="Stand" beats="2" />
      <move select="Flip Right" beats="5.5" />
      <move select="Cast Left" hands="gripleft"/>
      <move select="Stand" beats="3" />
      <move select="Cast Left" />
    </path>
  </tam>

  <tam title="Finish Spin Chain Thru" group="Finish"
       formation="Ocean Waves RH BGGB" parts="4.5;3">
    <path>
    </path>
    <path>
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Cast Left" />
    </path>
    <path>
      <move select="Cast Left" />
      <move select="Stand" beats="3" />
      <move select="Cast Left" />
    </path>
    <path>
    </path>
  </tam>

  <tam title="Finish Swing the Fractions" group="Finish"
       formation="Ocean Waves RH BGGB"
       parts="3;4.5;3" >
    <path>
      <move select="Stand" beats="3" />
      <move select="Cast Right" />
      <move select="Stand" beats="3" />
      <move select="Hinge Right" />
    </path>
    <path>
      <move select="Swing Left" />
      <move select="Cast Right" />
      <move select="Stand" beats="3" />
      <move select="Hinge Right" />
    </path>
    <path>
      <move select="Swing Left" />
      <move select="Cast Right" />
      <move select="Swing Left" />
      <move select="Hinge Right" />
    </path>
    <path>
      <move select="Stand" beats="3" />
      <move select="Cast Right" />
      <move select="Swing Left" />
      <move select="Hinge Right" />
    </path>
  </tam>

  <tam title="Finish Tally Ho" group="Finish"
       formation="Tidal Wave RH BGGB"
       parts="3" >
    <path>
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="Hinge Left" scaleY=".5" />
    </path>
    <path>
      <move select="Swing Right" scaleX=".5" scaleY=".5" />
      <move select="Lead Right" beats="4" scaleX="3" scaleY="1.5" />
    </path>
    <path>
      <move select="Hinge Right" scaleY=".5" />
      <move select="Extend Left" beats="4" scaleX="3" scaleY="2" />
    </path>
    <path>
      <move select="Hinge Right" scaleY=".5" />
      <move select="Lead Right" scaleX=".5" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="Hinge Left" scaleY=".5" />
    </path>
  </tam>

  <tam title="Finish Triple Star Thru" group="Finish"
       formation="Normal Lines" parts="3.5" >
    <path>
      <move select="Extend Right" hands="left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Quarter Left" beats="1.5" offsetX="1" offsetY=".5" />
    </path>
    <path>
      <move select="Extend Right" hands="left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Quarter Right" beats="1.5" offsetX="1" offsetY=".5" />
      <move select="Extend Left" hands="right" beats="1.5" scaleY=".5" />
      <move select="Quarter Left" beats="1.5" offsetX="1" offsetY="-.5" />
    </path>
    <path>
      <move select="Extend Right" hands="left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Quarter Left" beats="1.5" offsetX="1" offsetY=".5" />
      <move select="Extend Left" hands="right" beats="1.5" scaleY=".5" />
      <move select="Quarter Right" beats="1.5" offsetX="1" offsetY="-.5" />
    </path>
    <path>
      <move select="Extend Right" hands="left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Quarter Right" beats="1.5" offsetX="1" offsetY=".5" />
    </path>
  </tam>


</tamination>
